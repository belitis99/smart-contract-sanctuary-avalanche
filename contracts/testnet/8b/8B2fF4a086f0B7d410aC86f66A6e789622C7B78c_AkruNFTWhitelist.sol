// import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "./IWhitelistNFT.sol";
import "hardhat/console.sol";
/**
 * @title AkruAdminWhitelist
 * @author Umar Mubeen
 * @dev AkruAdminWhitelist to whitlist of AKRU's users
 */
contract AkruNFTAdminWhitelist is
    Ownable
{
    
    IWhitelistNFT public NFT; //ERC721 being used in whitelisting
    address public superAdmin;
    address feeAddress;
    address subSuperAdmin;
    bool public accreditationCheck = true;

    //store the prefix of NFT id associated with each role
    mapping(ROLES => uint256) public roleIdPrefix;
    //store the each role struct against id
    mapping(uint256 => userRole) public roleInfo;

      /**
     * @dev enum of different roles
     */
    enum ROLES {
        unRegister,
        superAdmin,
        subSuperAdmin,
        admin,
        manager,
        mediaManager,
        propertyOwner,
        propertyAccount,
        propertyManager,
        serviceProvider,
        subServiceProvider,
        bank,
        user_USA,
        user_Foreign,
        user_USAPremium,
        user_ForeignPremium
    }

    /**
    *@dev it takes NFT address and then mint nft to create super admin 
        add and entry to roleInfo mapping. 
    */
    constructor(address nftAddress) {
        require(
            !Address.isContract(_msgSender()),
            "Admin-Whitelist: Super admin cant be Contract address"
        );
        NFT =IWhitelistNFT(nftAddress);
        superAdmin = _msgSender();
        initializeRoleIdPrefix();
    }

    /**
     * @dev struct to store role related information
     */
    struct userRole {
        uint256 roleId;
        ROLES roleName;
        // uint256 NFTID;
        address userAddress;
        uint256 idPrefix;
        bool valid;
    }

    event AddWhitelistRole(
        address user,
        uint256 nftId,
        ROLES role,
        address caller
    );

    event AddSuperAdmin(address user, uint256 nftId, uint256 roleInfoId);

    event SetRoleIdPrefix(ROLES role, uint256 prefix);

    event UpdateAccreditationCheck(bool status, address caller);

    event RemoveWhitelistedRole(address user, ROLES role, uint256 roleInfoId);
    event AddFeeAddress(address feeAddress, address sender);


    /**
     * @dev method works as modifier to allow only super admin
     */
    function onlySuperAdmin() internal view {
        bool isSuperAdminRange = checkAdminRole(_msgSender(), ROLES.superAdmin);
        require(
            (isSuperAdminRange),
            "Admin-Whitelist: Only super admin is allowed"
        );
    }
    /**
     * @dev method works as modifier to allow only superAdmin & sub-superAdmin
     */
    function onlySuperAndSubSuperAdmin() internal view {
        bool isSuperAdminRange = checkAdminRole(_msgSender(), ROLES.superAdmin);
        bool isSubSuperAdminRange = checkAdminRole(
            _msgSender(),
            ROLES.subSuperAdmin
        );
        require(
            (isSuperAdminRange || isSubSuperAdminRange),
            "Admin-Whitelist: Only super OR Sub-super admins are authorized"
        );
    }

    /**
     * @dev method works as modifier to allow only superAdmin, sub-superAdmin and admin
     */
    function onlyAdmin() internal view {
        bool isSuperAdminRange = checkAdminRole(_msgSender(), ROLES.superAdmin);
        bool isSubSuperAdminRange = checkAdminRole(
            _msgSender(),
            ROLES.subSuperAdmin
        );
        bool isAdminRange = checkAdminRole(_msgSender(), ROLES.admin);
        require(
            (isSuperAdminRange || isSubSuperAdminRange || isAdminRange),
            "Admin-Whitelist: Only admin is allowed"
        );
    }
    /**
        * @dev method works as modifier to allow only 
            superAdmin, sub-superAdmin,admin and manager 
    */
    function onlyManager() internal view {
        bool isSuperAdminRange = checkAdminRole(_msgSender(), ROLES.superAdmin);
        bool isSubSuperAdminRange = checkAdminRole(
            _msgSender(),
            ROLES.subSuperAdmin
        );
        bool isAdminRange = checkAdminRole(_msgSender(), ROLES.admin);
        bool isManagerRange = checkAdminRole(_msgSender(), ROLES.manager);

        require(
            (isSuperAdminRange ||
                isSubSuperAdminRange ||
                isAdminRange ||
                isManagerRange),
            "Admin-Whitelist: Only Manager is allowed"
        );
    }

    /**
        * @dev method works as modifier to allow only 
            superAdmin, sub-superAdmin,admin and media-manager
    */
    function onlyMediaManager() internal view {
        bool isSuperAdminRange = checkAdminRole(_msgSender(), ROLES.superAdmin);
        bool isSubSuperAdminRange = checkAdminRole(
            _msgSender(),
            ROLES.subSuperAdmin
        );
        bool isAdminRange = checkAdminRole(_msgSender(), ROLES.admin);
        bool isMediaManager = checkAdminRole(_msgSender(), ROLES.mediaManager);

        require(
            (isSuperAdminRange ||
                isSubSuperAdminRange ||
                isAdminRange ||
                isMediaManager),
            "Admin-Whitelist: Only Media Manager is allowed"
        );
    }
    /**
     *@dev it only allow user with specific role to add other roles.
     *@param user user address to assign role
     *@param nftId to be associated with role.
     *@param role that willbe assigned.
     */
    function addWhitelistRole(address user, uint256 nftId, ROLES role) public {
        if (role == ROLES.superAdmin || role == ROLES.subSuperAdmin) {
            onlySuperAdmin();
        }
        if (role == ROLES.admin) {
            onlySuperAndSubSuperAdmin();
        }

        if (
            role == ROLES.manager ||
            role == ROLES.mediaManager ||
            role == ROLES.bank ||
            role == ROLES.propertyAccount ||
            role == ROLES.propertyOwner ||
            role == ROLES.propertyManager ||
            role == ROLES.serviceProvider ||
            role == ROLES.subServiceProvider
        ) {
            onlyAdmin();
        }

        require(
            checkIdLength(nftId),
            "Admin-Whitelist: Id length must be greater than or equal to 2"
        );
        require(
            checkRoleToId(role, nftId),
            "Admin-Whitelist: The ID must fall under the Role Id category"
        );
        require(
            !(checkAdminRole(user, role)),
            "Admin-Whitelist: User already assigned the role"
        );

        NFT.mintToken(user, nftId);
        roleInfo[nftId] = userRole({
            roleId: nftId,
            roleName: role,
            // NFTID: NFTId,
            userAddress: user,
            idPrefix: getRoleIdPrefix(role),
            valid: true
        });

        emit AddWhitelistRole(user, nftId, role,_msgSender());
    }

    /**
     *@dev it only allow super admin to remove roles.
     *@param user that need to be removed from role.
     *@param role that will be removed.
     */
    function removeWhitelistedRole(address user, ROLES role) public {
        if (
            role == ROLES.superAdmin ||
            role == ROLES.subSuperAdmin ||
            role == ROLES.admin
        ) {
            onlySuperAdmin();
        }
        if (
            role == ROLES.manager ||
            role == ROLES.mediaManager ||
            role == ROLES.bank ||
            role == ROLES.propertyAccount ||
            role == ROLES.propertyOwner ||
            role == ROLES.propertyManager ||
            role == ROLES.serviceProvider ||
            role == ROLES.subServiceProvider
        ) {
            onlyAdmin();
        }
        require(
            checkAdminRole(user, role),
            "Admin-Whitelist: Role does not exist"
        );
        uint256 rolePrefix = getRoleIdPrefix(role);
        uint256[] memory nftIds = NFT.getOwnedTokenIds(user);
        uint256 roleInfoId;
        for (uint i = 0; i < nftIds.length; ) {
            uint256 idPrefix = getIdPrefix(nftIds[i]);
            if (rolePrefix == idPrefix) {
                roleInfoId = nftIds[i];
                userRole storage roleData = roleInfo[nftIds[i]];
                // roleData.NFTID = 0;
                roleData.valid = false;

                NFT.burnToken(nftIds[i]);
            }
            unchecked {
                i++;
            }
        }

        emit RemoveWhitelistedRole(user, role, roleInfoId);
    }

    /**
     *@dev add the super admin role , 
        this method willbe called just after deployment of the whitelisting smart contract
     */
    function addSuperAdmin() external onlyOwner {
        NFT.mintToken(_msgSender(), 11);
        roleInfo[11] = userRole({
            roleId: 11,
            roleName: ROLES.superAdmin,
            // NFTID: 11,
            userAddress: _msgSender(),
            idPrefix: getRoleIdPrefix(ROLES.superAdmin),
            valid: true
        });

        emit AddSuperAdmin(_msgSender(), 11, 11);
    }

    /**
     * @dev Update Accredential Status
     * @param status true/false
     */
    function updateAccreditationCheck(bool status) public {
        onlyManager();
        accreditationCheck = status;
        emit UpdateAccreditationCheck(status, _msgSender());
    }

    /**
     * @dev check the media manager role
     * @param _wallet  Address of manager
     * @return role status
     */
    function isMediadManager(address _wallet) public view returns (bool) {
        return checkAdminRole(_wallet, ROLES.mediaManager);
    }

    /**
     * @dev whitelist the feeAddress in AKRU
     * @param _feeAddress address of new fee address
     */
    function addFeeAddress(address _feeAddress) public {
        //onlySuperAndSubSuperAdmin

        feeAddress = _feeAddress;
        emit AddFeeAddress(_feeAddress, msg.sender);
    }

    /**
     * @dev return the feeAddress in AKRU
     * @return feeAddress
     */
    function getFeeAddress() public view returns (address) {
        return feeAddress;
    }

    /**
     * @dev return the status of admin in AKRU
     * @param _calle address of admin
     * @return true/false
     */
    function isAdmin(address _calle) public view returns (bool) {
        return checkAdminRole(_calle, ROLES.admin);
    }

    /**
     * @dev return the status of super admin in AKRU
     * @param _calle address of super admin
     * @return true/false
     */
    function isSuperAdmin(address _calle) public view returns (bool) {
        return checkAdminRole(_calle, ROLES.superAdmin);
    }

    /**
     * @dev return the status of sub super admin in AKRU
     * @param _calle address of sub super admin
     * @return true/false
     */
    function isSubSuperAdmin(address _calle) public view returns (bool) {
        return checkAdminRole(_calle, ROLES.subSuperAdmin);
    }

    /**
     * @dev return the status of bank in AKRU
     * @param _calle address of bank
     * @return true/false
     */
    function isBank(address _calle) public view returns (bool) {
        return checkAdminRole(_calle, ROLES.bank);
    }

    /**
     * @dev return the status of property owner in AKRU
     * @param _calle address of property owner
     * @return true/false
     */
    function isOwner(address _calle) public view returns (bool) {
        return checkAdminRole(_calle, ROLES.propertyOwner);
    }

    /**
     * @dev return the status of manager in AKRU
     * @param _calle address of manager
     * @return true/false
     */
    function isManager(address _calle) public view returns (bool) {
        return checkAdminRole(_calle, ROLES.manager);
    }

    /**
     * @dev return role related information against id
     * @param roleId of struct
     */
    function getRoleInfo(
        uint256 id
    )
        public
        view
        returns (
            uint256 roleId,
            ROLES roleName,
            // uint256 NFTID,
            address userAddress,
            uint256 idPrefix,
            bool valid
        )
    {
        userRole memory role = roleInfo[id];
        return (
            role.roleId,
            role.roleName,
            // role.NFTID,
            role.userAddress,
            role.idPrefix,
            role.valid
        );
    }

    /**
     * @dev check if the user has specific role
     * @param userAddress address of user
     * @param role to be checked
     * @return true/false
     */
    function checkAdminRole(
        address userAddress,
        ROLES role
    ) internal view returns (bool) {
        uint256 rolePrefix = getRoleIdPrefix(role);
        uint256[] memory nftIds = NFT.getOwnedTokenIds(userAddress);
        for (uint i = 0; i < nftIds.length; ) {
            uint256 idPrefix = getIdPrefix(nftIds[i]);
            if (rolePrefix == idPrefix) {
                return true;
            }
            unchecked {
                i++;
            }
        }
        return false;
    }

    /**
     * @dev check if the given nft Id falls under specific role
     * @param id nft id
     * @param role to be checked
     * @return true/false
     */
    function checkRoleToId(
        ROLES role,
        uint256 id
    ) internal view returns (bool) {
        uint256 rolePrefix = getRoleIdPrefix(role);
        uint256 idPrefix = getIdPrefix(id);
        return rolePrefix == idPrefix;
    }

    /**
     * @dev set the nft ID prefix (2 digits) for specific role.
     * @param IdPrefix nft id prefix value
     * @param role associated role with nft id prefix.
     */
    function setRoleIdPrefix(ROLES role, uint256 IdPrefix) public {
        roleIdPrefix[role] = IdPrefix;
        emit SetRoleIdPrefix(role, IdPrefix);
    }

    /**
     * @dev return the NFTId prefix of given role
     * @param role to check prefix.
     */
    function getRoleIdPrefix(ROLES role) public view returns (uint256) {
        return roleIdPrefix[role];
    }

    /**
     * @dev it returs the first 2 digit of NFT id.
     * @param id nft id to get first 2 digits
     */
    function getIdPrefix(uint id) internal pure returns (uint) {
        uint divisor = 1;
        while (id / divisor >= 100) {
            divisor *= 10;
        }
        uint result = id / divisor;
        return result % 100;
    }

    /**
     * @dev it ensures that the given nft Id must be greater or equak to 2 digits
     * @param id nft id to verify length
     */
    function checkIdLength(uint256 id) internal pure returns (bool) {
        bytes memory idBytes = abi.encodePacked(id);
        return idBytes.length >= 2;
    }

    /**
     * @dev it initializes and associate the each role with specific ID prefix.
     */
    function initializeRoleIdPrefix() internal {
        // setRoleIdPrefix(ROLES.unRegistered, 00);
        setRoleIdPrefix(ROLES.superAdmin, 11);
        setRoleIdPrefix(ROLES.subSuperAdmin, 12);
        setRoleIdPrefix(ROLES.admin, 13);
        setRoleIdPrefix(ROLES.manager, 14);
