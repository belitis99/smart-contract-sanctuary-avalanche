pragma solidity ^0.8.19;

import "./AkruNFTAdminWhitelist.sol";

/**
 * @title AkruWhitelist
 * @dev AkruWhitelist to whitlist of AKRU's users
 */
contract AkruNFTWhitelist is AkruNFTAdminWhitelist
{
    address[] public userList;

    mapping(string => bool) symbolsDef;
    mapping(address => whitelistInfo) public whitelistUsers;

    constructor(
        address nftAddress
    ) AkruNFTAdminWhitelist(nftAddress) {}

    ///user information
    struct whitelistInfo {
        bool valid;
        address wallet;
        bool kycVerified;
        bool accredationVerified;
        uint256 accredationExpiry;
        uint256 taxWithholding;
        ROLES Role;
        uint256 userRoleId;
    }

    event RemoveWhitelistedUser(address user, ROLES role, uint256 roleInfoId);

    event AddWhitelistUser(address user, ROLES role, uint256 NFTID, uint256 roleInfoId);
    event UpdateKycWhitelistedUser(address user, bool status);

    event UpdateAccredationWhitelistedUser(address user, uint256 accredation);

    event UpdateTaxWhitelistedUser(address user, uint256 taxWithholding);

    event UpdateUserAccredationStatus(address caller, address user, bool status);
    /**
     * @dev whitelist the user with specfic KYC and accredation verified check and time in AKRU
     * @param _wallet  Address of new user
     * @param _kycVerified is kyc true/false
     * @param _accredationVerified is accredation true/false
     * @param _accredationExpiry accredation expiry date in unix time
     */
    function addWhitelistUser(
        address _wallet,
        bool _kycVerified,
        bool _accredationVerified,
        uint256 _accredationExpiry,
        ROLES role,
        uint256 nftId
    ) public {
        onlyManager();
        // if (_accredationVerified)
        //     require(
        //         _accredationExpiry >= block.timestamp,
        //         "User-Whitelist: Accredation Expiry time is before current time"
        //     );
        require(
            checkIdLength(nftId),
            "User-Whitelist: Id length must be greater than or equal to 2"
        );
        require(
            checkRoleToId(role, nftId),
            "User-Whitelist: The ID must fall under the Role Id category"
        );
        require(
            !(checkUserRole(_wallet, role)),
            "User-Whitelist: User already assigned the role"
        );
        NFT.mintToken(_wallet, nftId);
        userList.push(_wallet);
        roleInfo[nftId] = userRole({
            roleId: nftId,
            roleName: role,
            // NFTID: nftId,
            userAddress: _wallet,
            idPrefix: 11,
            valid: true
        });
        whitelistUsers[_wallet] = whitelistInfo({
            valid: true,
            taxWithholding: 0,
            wallet: _wallet,
            kycVerified: _kycVerified,
            accredationExpiry: _accredationExpiry,
            accredationVerified: _accredationVerified,
            Role: role,
            userRoleId: nftId
        });
        emit AddWhitelistUser(_wallet, role, nftId, nftId);
    }

      /**
     * @dev check if the user has specific role
     * @param userAddress address of user
     * @param role to be checked
     * @return true/false
     */
    function checkUserRole(
        address userAddress,
        ROLES role
    ) public view returns (bool) {
        uint256 rolePrefix = getRoleIdPrefix(role);
        uint256[] memory nftIds = NFT.getOwnedTokenIds(userAddress);
        for (uint i = 0; i < nftIds.length; ) {
            uint256 idPrefix = getIdPrefix(nftIds[i]);
            if (rolePrefix == idPrefix) {
                if(role == ROLES.user_USA || role == ROLES.user_USAPremium){
                  return whitelistUsers[userAddress].accredationExpiry >= block.timestamp;
                }
                return true;
            }
            unchecked {
                i++;
            }
        }
        return false;
    }

    /**
     * @dev get the user with specfic KYC and accredation verified check and time in AKRU
     * @param _wallet  Address of  user
     * @return _wallet  Address of  user
     * @return _kycVerified is kyc true/false
     * @return _accredationVerified is accredation true/false
     * @return _accredationExpiry accredation expiry date in unix time
     */
    function getWhitelistedUser(
        address _wallet
    )
        public
        view
        returns (address, bool, bool, uint256, ROLES, uint256, uint256, bool)
    {
        whitelistInfo memory u = whitelistUsers[_wallet];
        return (
            u.wallet,
            u.kycVerified,
            u.accredationExpiry >= block.timestamp,
            u.accredationExpiry,
            u.Role,
            u.taxWithholding,
            u.userRoleId,
            u.valid
        );
    }

    /**
     * @dev remove the whitelisted user , burn nft and update structs
     * @param user  Address of user being removed
     * @param role role that need to be removed.
     */

    function removeWhitelistedUser(address user, ROLES role) public {
        onlyAdmin();
        require(
            checkUserRole(user, role),
            "NFT-Whitelist: Role not assigned to user"
        );
        uint256 rolePrefix = getRoleIdPrefix(role);
        uint256[] memory nftIds = NFT.getOwnedTokenIds(user);
        whitelistInfo storage userData = whitelistUsers[user];
        uint256 roleInfoId;
        for (uint i = 0; i < nftIds.length; ) {
            uint256 idPrefix = getIdPrefix(nftIds[i]);
            if (rolePrefix == idPrefix) {
                roleInfoId = nftIds[i];
                userRole storage roleData = roleInfo[nftIds[i]];
                // roleData.NFTID = 0;
                roleData.valid = false;
                userData.valid = false;

                NFT.burnToken(nftIds[i]);
            }
            unchecked {
                i++;
            }
        }

        emit RemoveWhitelistedUser(user, role, roleInfoId);
    }

    /**
     * @dev update the user with specfic KYC verified check in AKRU
     * @param _wallet  Address of user
     * @param _kycVerified is kyc true/false
     */
    function updateKycWhitelistedUser(
        address _wallet,
        bool _kycVerified
    ) public {
        onlyAdmin();
        whitelistInfo storage u = whitelistUsers[_wallet];
        u.kycVerified = _kycVerified;

        emit UpdateKycWhitelistedUser(_wallet, _kycVerified);
    }

    /**
     * @dev update the user whitelisted user Accredation Status
     * @param _wallet  Address of user
     * @param AccredationStatus is Accredation true/false
     */
    function updateUserAccredationStatus(
        address _wallet,
        bool AccredationStatus
    ) public {
        onlyAdmin();
        whitelistInfo storage u = whitelistUsers[_wallet];
        u.accredationVerified = AccredationStatus;

        emit UpdateUserAccredationStatus(_msgSender(), _wallet, AccredationStatus);
    }

    /**
     * @dev update the user with specfic accredation expiry date in AKRU
     * @param _wallet  Address of user
     * @param  _accredationExpiry accredation expiry date in unix time
     */
    function updateAccredationWhitelistedUser(
        address _wallet,
        uint256 _accredationExpiry
    ) public {
        onlyAdmin();
        require(
            _accredationExpiry >= block.timestamp,
            "NFT-Whitelist: Accredation Expiry time is before current time"
        );
        whitelistInfo storage u = whitelistUsers[_wallet];
        u.accredationExpiry = _accredationExpiry;

        emit UpdateAccredationWhitelistedUser(_wallet, _accredationExpiry);
    }

    /**
     * @dev update the user with new tax holding in AKRU
     * @param _wallet  Address of user
     * @param  _taxWithholding  new taxWithholding
     */
    function updateTaxWhitelistedUser(
        address _wallet,
        uint256 _taxWithholding
    ) public {
        onlyAdmin();
        whitelistInfo storage u = whitelistUsers[_wallet];
        u.taxWithholding = _taxWithholding;
        emit UpdateTaxWhitelistedUser(_wallet, _taxWithholding);
    }

    /**
     * @dev Symbols Deployed Add to Contract
     * @param _symbols string of new symbol added in AKRU
     */
    function addSymbols(string calldata _symbols) external returns (bool) {
        if (symbolsDef[_symbols] == true) return false;
        else {
            symbolsDef[_symbols] = true;
            return true;
        }
    }

    /**
     * @dev removed Symbol from Contract
     * @param _symbols string of already added symbol in AKRU
     */
    function removeSymbols(string calldata _symbols) external returns (bool) {
        onlyManager();
        if (symbolsDef[_symbols] == true) symbolsDef[_symbols] = false;
        return true;
    }

    /**
     * @dev returns the status of kyc of user.
     * @param user address to verify the KYC.
     */
    function isKYCverfied(address user) public view returns (bool) {
        whitelistInfo memory info = whitelistUsers[user];
        return info.kycVerified;
    }

    /**
     * @dev returns the status of accredation of user.
     * @param user address to verify the accredation.
     */
    function isAccreditationVerfied(address user) public view returns (bool) {
        whitelistInfo memory info = whitelistUsers[user];
        return info.accredationExpiry > block.timestamp;
    }

    /**
     * @dev returns the status of accredetation of user.
     * @param user address to check the accredetation.
     */
    function isAccredatitationExpired(address user) public view returns (bool) {
        whitelistInfo memory info = whitelistUsers[user];
        return info.accredationExpiry < block.timestamp;
    }

    /**
     * @dev return the whitelist contract all stored data
     * @return _userList list of the user
     * @return _validity is user valid
     * @return _kycVery is KYC is valid
     * @return _accredationVery is _accredation Valid
     * @return _accredationExpir accredation expiry date of user
     * @return _taxWithHold  hold tax of the user
     */
    function getAllUserStoredData()
        public
        view
        returns (
            address[] memory _userList,
            bool[] memory _validity,
            bool[] memory _kycVery,
            bool[] memory _accredationVery,
            uint256[] memory _accredationExpir,
            uint256[] memory _taxWithHold
        )
    {
        onlyAdmin();
        uint256 size = userList.length;

        bool[] memory validity = new bool[](size);
        bool[] memory kycVery = new bool[](size);
        bool[] memory accredationVery = new bool[](size);
        uint256[] memory accredationExpir = new uint256[](size);
        uint256[] memory taxWithHold = new uint256[](size);
        uint256 i;
        for (i = 0; i < userList.length; i++) {
            if (whitelistUsers[userList[i]].valid) {
                validity[i] = true;
            } else {
                validity[i] = false;
            }
            if (whitelistUsers[userList[i]].kycVerified) {
                kycVery[i] = true;
            } else {
                kycVery[i] = false;
            }
            if (whitelistUsers[userList[i]].accredationVerified) {
                accredationVery[i] = true;
            } else {
                accredationVery[i] = false;
            }
            accredationExpir[i] = (
                whitelistUsers[userList[i]].accredationExpiry
            );
            taxWithHold[i] = (whitelistUsers[userList[i]].taxWithholding);
        }
        return (
            userList,
            validity,
            kycVery,
            accredationVery,
            accredationExpir,
            taxWithHold
        );
    }

    /**
     * @dev return the status of USA user in AKRU
     * @param user address of sub super admin
     * @return true/false
     */
    function isUserUSA(address user) public view returns (bool) {
        return checkUserRole(user, ROLES.user_USA);
    }

    /**
     * @dev return the status of Foreign user in AKRU
     * @param user address of sub super admin
     * @return true/false
     */
    function isUserForeign(address user) public view returns (bool) {
        return checkUserRole(user, ROLES.user_Foreign);
    }

    /**
     * @dev return the type of the user
     * @param _caller address of the user
     * @return _userTypes type of the user i.e Basic or Premium , USA , Foreign
     */
    function getUserType(address _caller) public view returns (string memory) {
        ROLES userType = whitelistUsers[_caller].Role;

        if (userType == ROLES.user_USAPremium) {
            return "USA Premium User";
        } else if (userType == ROLES.user_USA) {
            return "USA User";
        } else if (userType == ROLES.user_ForeignPremium) {
            return "Foreign Premium User";
        } else if (userType == ROLES.user_Foreign) {
            return "Foreign User";
        } else {
            return "Unknown User Type";
        }
    }

    /**
     * @dev return white listed user information
     * @param user address of the user
     */
    function getWhitelistInfo(
        address user
    )
        public
        view
        returns (
            bool valid,
            address wallet,
            bool kycVerified,
            bool accredationVerified,
            uint256 accredationExpiry,
            uint256 taxWithholding,
            ROLES role,
            uint256 userRoleId
        )
    {
        whitelistInfo memory info = whitelistUsers[user];
        return (
            info.valid,
            info.wallet,
            info.kycVerified,
            info.accredationVerified,
            info.accredationExpiry,
            info.taxWithholding,
            info.Role,
            info.userRoleId
        );
    }

    /**
     * @dev return the role and role id hold by the given user
     * @param _userAddress address of the user.
     */
    function isWhitelistedUser(
        address _userAddress
    ) public view returns (uint256) {

        if (checkUserRole(_userAddress, ROLES.superAdmin)) {
            return 100;
        } else if (checkUserRole(_userAddress, ROLES.subSuperAdmin)) {
            return 101;
        } else if (checkUserRole(_userAddress, ROLES.admin)) {
            return 112;
        } else if (checkUserRole(_userAddress, ROLES.manager)) {
            return 120;
        } else if (checkUserRole(_userAddress, ROLES.mediaManager)) {
            return 125;
        } else if (checkUserRole(_userAddress, ROLES.bank)) {
            return 130;
        } else if (checkUserRole(_userAddress, ROLES.propertyAccount)) {
            return 132;
        } else if (checkUserRole(_userAddress, ROLES.propertyOwner)) {
            return 140;
        } else if (checkUserRole(_userAddress, ROLES.propertyManager)) {
            return 135;
        } else if (
            checkUserRole(_userAddress, ROLES.user_USAPremium) ||
            checkUserRole(_userAddress, ROLES.user_USA) ||
            checkUserRole(_userAddress, ROLES.user_ForeignPremium) ||
            checkUserRole(_userAddress, ROLES.user_Foreign) ||
            checkUserRole(_userAddress, ROLES.serviceProvider) ||
            checkUserRole(_userAddress, ROLES.subServiceProvider)
        ) {
            return 200;
        } else {
            return 400;
        }
    }

    /**
     * @dev return the role and role id hold by the given user
     * @param _userAddress address of the user.
     */
    function getUserRole(
        address _userAddress
    ) public view returns (string memory roleName, ROLES value) {
        
        if (checkUserRole(_userAddress, ROLES.superAdmin)) {
            return ("Super Admin", ROLES.superAdmin);
        } else if (checkUserRole(_userAddress, ROLES.subSuperAdmin)) {
            return ("Sub-Super Admin", ROLES.subSuperAdmin);
        } else if (checkUserRole(_userAddress, ROLES.admin)) {
            return ("Admin", ROLES.admin);
        } else if (checkUserRole(_userAddress, ROLES.manager)) {
            return ("Manager", ROLES.manager);
        } else if (checkUserRole(_userAddress, ROLES.mediaManager)) {
            return ("Media Manager", ROLES.mediaManager);
        } else if (checkUserRole(_userAddress, ROLES.bank)) {
            return ("Bank", ROLES.bank);
        } else if (checkUserRole(_userAddress, ROLES.propertyAccount)) {
            return ("Property Account", ROLES.propertyAccount);
        } else if (checkUserRole(_userAddress, ROLES.propertyOwner)) {
            return ("Property Owner", ROLES.propertyOwner);
        } else if (checkUserRole(_userAddress, ROLES.propertyManager)) {
            return ("Property Manager", ROLES.propertyManager);
        } else if (checkUserRole(_userAddress, ROLES.user_USAPremium)) {
            return ("USA Premium User", ROLES.user_USAPremium);
        } else if (checkUserRole(_userAddress, ROLES.user_USA)) {
            return ("USA User", ROLES.user_USA);
        } else if (checkUserRole(_userAddress, ROLES.user_ForeignPremium)) {
            return ("Foreign Premium User", ROLES.user_ForeignPremium);
        } else if (checkUserRole(_userAddress, ROLES.user_Foreign)) {
            return ("Foreign User", ROLES.user_Foreign);
        } else if (checkUserRole(_userAddress, ROLES.serviceProvider)) {
            return ("Service Provider", ROLES.serviceProvider);
        } else if (checkUserRole(_userAddress, ROLES.subServiceProvider)) {
            return ("Sub-Service Provider", ROLES.subServiceProvider);
        } else{
            return ("User not registered", ROLES.unRegister);
        }
    
    }

    /**
     * @dev destroy the whitelist contract
     */
    function closeTokenismWhitelist() public {
        onlySuperAdmin();
        selfdestruct(payable(msg.sender));
    }
}

