        setRoleIdPrefix(ROLES.user_ForeignPremium, 22);
        setRoleIdPrefix(ROLES.user_Foreign, 23);
        setRoleIdPrefix(ROLES.serviceProvider, 24);
        setRoleIdPrefix(ROLES.subServiceProvider, 25);
    }
}

