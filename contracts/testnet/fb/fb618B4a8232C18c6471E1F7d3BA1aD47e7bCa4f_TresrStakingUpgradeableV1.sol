import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "../VeTresrToken.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "../StakingBase.sol";
import "../interfaces/IReward.sol";
import "hardhat/console.sol";

interface IVeTresr {
    function mint(address to, uint256 amount) external;
    function resetBalance(address _from) external;
    function balanceOf(address _user) external view returns (uint256);
    function totalSupply() external view returns (uint256);
}

contract TresrStakingUpgradeableV1 is ReentrancyGuardUpgradeable, OwnableUpgradeable, StakingBase {

    address private _tokenAddressContract;
    address private _rewardContractAddress;
    uint private _stakedAll;
    uint private _rewardConst;
    IVeTresr public veTresr;
    uint256 constant VETRESR_PERSEC = 5560000000000;
    mapping(address => Reward) private stakedRewardList; // FIX: this should be removed
    
    struct Reward {
        uint lastStakeDate;
        uint staked;
        uint rewards;
    }

    function initialize(address newTokenAddressContract, address newRewardAddressContract, address veTresrAddress) initializer public {
        __Ownable_init();

        _tokenAddressContract = newTokenAddressContract;
        _rewardContractAddress = newRewardAddressContract;
        _rewardConst = 2;
        veTresr = IVeTresr(veTresrAddress);
    }

    modifier moreThanZero(uint amount) {
        require(amount > 0, "TresrStaking: amount must be more than zero");
        _;
    }

    modifier onlyRewardContract() {
        require(_rewardContractAddress == msg.sender, "TresrStaking: caller is not BonusRewardContract");
        _;
    }

    function setRewardConst(uint amount) external onlyOwner() {
        _rewardConst = amount;
    }

    function getRewardConst() external view onlyOwner() returns (uint) {
        return _rewardConst;
    }

    // TODO: should reflect in the frontend after deployment
    function getStaked(address user) public view returns (uint) {
        return stakedRewardList[user].staked;
    }

    function getStakedAll() public view returns (uint) {
        return _stakedAll;
    }
   
    function stake(uint amount) external moreThanZero(amount) {
        IERC20Upgradeable(_tokenAddressContract).transferFrom(msg.sender, address(this), amount);

        if (stakedRewardList[msg.sender].lastStakeDate != 0) {
            stakedRewardList[msg.sender].rewards += _pendingVeTresr(msg.sender);
        }

        stakedRewardList[msg.sender].staked += amount;
        stakedRewardList[msg.sender].lastStakeDate = block.timestamp;

        _stakedAll += amount;
    }

    function unstake(uint amount) external moreThanZero(amount) {
        
        require(stakedRewardList[msg.sender].staked >= amount, "TresrStaking: staked must be more than amount");
        claimedAmount[msg.sender] += _pendingBonusReward(msg.sender);
        uint256 oldBalance = veTresr.totalSupply();

        IERC20Upgradeable(_tokenAddressContract).transfer(msg.sender, amount);

        stakedRewardList[msg.sender].rewards = 0;
        stakedRewardList[msg.sender].staked -= amount;
        stakedRewardList[msg.sender].lastStakeDate = block.timestamp;

        _stakedAll -= amount;
        veTresr.resetBalance(msg.sender);
        _updateMathParam(oldBalance, msg.sender);

    }
    
    function claimVeTresr() external {
        claimedAmount[msg.sender] += _pendingBonusReward(msg.sender);
        uint256 oldBalance = veTresr.totalSupply();
        uint256 amount = _pendingVeTresr(msg.sender);

        stakedRewardList[msg.sender].rewards = 0;
        stakedRewardList[msg.sender].lastStakeDate = block.timestamp;

        veTresr.mint(msg.sender, amount);
        _updateMathParam(oldBalance, msg.sender);
    }

    function daysToMax(address _user) external view returns (uint256) {
        uint256 balance = _stakedBalanceOf(_user) + _pendingVeTresr(_user);
        return(_maxVeTresr(_user) - balance) / (veTresrPerHour(_user) * 24) ;
    }

    function maxVeTresr(address _user) external view returns (uint256) {
        return _maxVeTresr(_user);
    }

    function _maxVeTresr(address _user) internal view returns (uint256) {
        return stakedRewardList[_user].staked * 100;
    }

    function _pendingVeTresr(address _user) internal view returns (uint256) {
        // uint rewardMax = getRewardMax();
        if(_stakedBalanceOf(_user) >= _maxVeTresr(_user)) return 0;
        uint rewardSum = stakedRewardList[_user].rewards; // stacked veTreasure reward
        // (timestamp - lastclaimedtime) * tresrAmount * vetresrPerSec
        if (stakedRewardList[_user].lastStakeDate != 0) { // calculate newly stacked reward
            rewardSum += (block.timestamp - stakedRewardList[_user].lastStakeDate) * VETRESR_PERSEC * stakedRewardList[_user].staked / 1e18;
        }
        if(rewardSum + _stakedBalanceOf(_user) > _maxVeTresr(_user)) {
            rewardSum = rewardSum + _stakedBalanceOf(_user) - _maxVeTresr(_user);
        }
        return rewardSum;
    }


    function veTresrPerHour(address _user) public view returns (uint256) {
        return 3600 * VETRESR_PERSEC * stakedRewardList[_user].staked / 1e18;
    }
    

    function pendingVeTresr(address _user) external view returns (uint256) {
        return _pendingVeTresr(_user);
    }

    /// StakingBase.sol implementation
    
    function pendingBonusReward(address _user) override external view returns (uint) {

        return _pendingBonusReward(_user) + claimedAmount[_user];
    }
    function _pendingBonusReward(address _user) internal view returns (uint) {
        uint256 balance = _stakedBalanceOf(_user);
        uint256 userPortion = _portion(_user); 

        // uint256 amount = mathParam.lossUnit * balance + currentRatio * (block.timestamp - mathParam.lastUpdated) * userPortion / 1000 - rewardedDebt[_user];
        uint256 amount = mathParam.lossUnit * balance;
        uint256 currentTime = block.timestamp;
        uint256 ratio = currentRatio;
        
        if (currentTime < nextDayTick) {
            amount += currentRatio * (block.timestamp - mathParam.lastUpdated) * userPortion / 1000;
        }
        else { // currentTime > nextDayTick 
            uint256 lastUpdated = mathParam.lastUpdated;
            uint256 lossUnit = mathParam.lossUnit;
            uint256 _nextDayTick = nextDayTick;
            while(currentTime > _nextDayTick) {
                amount += ratio * (_nextDayTick - lastUpdated) / 1000 * userPortion;
                lastUpdated = _nextDayTick;
                _nextDayTick += 1 days;
                ratio = IReward(_rewardContractAddress).dailyRewardRatio(lastUpdated);
            }
            lossUnit += ratio * (block.timestamp - lastUpdated) / 1000 * userPortion;
        }

        return amount - rewardedDebt[_user]; 
    }


    function portion(address _user) override external view returns (uint) {
        return _portion(_user);
    }
    function _portion(address _user) internal view returns (uint) {
        if(_totalStaked() == 0) return 0;
        return _stakedBalanceOf(_user) * 1e36 / _totalStaked();
    }

    function startRewarding() override external onlyRewardContract {
        nextDayTick = block.timestamp + 1 days;
        currentRatio = 1000;
    }

    function stakedBalanceOf(address _user) override external view returns (uint256) {
        return _stakedBalanceOf(_user);
    }

    function claimed(address _user) override external onlyRewardContract {
        rewardedDebt[_user] += _pendingBonusReward(_user);
        claimedAmount[_user] = 0;
    }

    function _stakedBalanceOf(address _user) internal view returns (uint256) {
        return veTresr.balanceOf(_user);
    }

    function totalStaked() override external view  returns  (uint256){
        return _totalStaked();
    }

    function _totalStaked() internal view  returns  (uint256){
        return veTresr.totalSupply();
    }

    function _updateMathParam(uint256 oldBalance, address _user) internal {
        // loss unit is loss of axiom percent, so it should be multiplied by user's staked amount
        // in computation
        // axiom percent is 100% / total balance, so axiom percent * amount is user's percent
        // if block.timestamp > nextintegra
        if (oldBalance != 0) {
            uint256 currentTime = block.timestamp;
            uint256 ratio = currentRatio;
            if (currentTime < nextDayTick) {
                mathParam.lossUnit += currentRatio * (block.timestamp - mathParam.lastUpdated) * 1e36 / 1000 / oldBalance;
            }
            else { // currentTime > nextDayTick 

                uint256 lastUpdated = mathParam.lastUpdated;
                uint256 lossUnit = mathParam.lossUnit;
                uint256 _nextDayTick = nextDayTick;
                while(currentTime > _nextDayTick) {
                    lossUnit += ratio * (_nextDayTick - lastUpdated) * 1e36 / 1000 / oldBalance;
                    lastUpdated = _nextDayTick;
                    _nextDayTick += 1 days;
                    ratio = IReward(_rewardContractAddress).dailyRewardRatio(lastUpdated);
                }
                lossUnit += ratio * (block.timestamp - lastUpdated) * 1e36 / 1000 / oldBalance;
                currentRatio = ratio;
                nextDayTick = _nextDayTick;
                mathParam.lossUnit = lossUnit;
            }
        }
        else { // there was bug due to the not proper update of nextDaytick when oldbalance is zero
            uint256 _nextDayTick = nextDayTick;
            while(block.timestamp > _nextDayTick) {
                _nextDayTick += 1 days;
            }
            nextDayTick = _nextDayTick;
            currentRatio = IReward(_rewardContractAddress).dailyRewardRatio(block.timestamp);
        }

        // why not +=, because rewardedDebt is claimed
        // but lossUnit always plused, rewardedDebt is also calculated from the 
        // very beginning timepoint
        // edge: wen rewardedDebt is zero again, and what happens?
        // it never get assigned to zero again. LoL
        if(_totalStaked() != 0)
            rewardedDebt[_user] = mathParam.lossUnit * _stakedBalanceOf(_user);
        else 
            rewardedDebt[_user] = 0;

        mathParam.lastUpdated = block.timestamp;
    }

    // TODO: this is used for getting total claim. :) this is only veTresur
    function getReward(uint time) public view returns (uint) {
        uint rewardMax = getRewardMax();
        uint rewardSum = stakedRewardList[msg.sender].rewards;

        if (stakedRewardList[msg.sender].lastStakeDate != 0) {
            if (time > stakedRewardList[msg.sender].lastStakeDate) {
                rewardSum += (time - stakedRewardList[msg.sender].lastStakeDate) * _calcRewardPerSecond(stakedRewardList[msg.sender].staked);
            }
        }

        if (rewardSum > rewardMax) return rewardMax;
        return rewardSum;
    }

    function _calcRewardPerSecond(uint value) internal view returns (uint) {
        return value * _rewardConst / 100 / 3600;
    }

    function getRewardPerSecond(address _user) public view returns (uint) {
        return _calcRewardPerSecond(stakedRewardList[_user].staked);
    }

    function getRewardMax() public view returns (uint) {
        return stakedRewardList[msg.sender].staked * 100;
    }
    


    // TODO: need to remove this
    function getVeTresrBonusRewardPerSecond(address account) public view onlyRewardContract returns (uint) {
        return _calcRewardPerSecond(stakedRewardList[account].staked);
    }

    // TODO: need to remove this
    // DAILY BONUS REWARD --------------------------------------------------
    function getVeTresrBonusReward(address account, uint time) public view onlyRewardContract returns (uint) {
        uint rewardMax = stakedRewardList[account].staked * 100;
        uint rewardSum = stakedRewardList[account].rewards;

        if (stakedRewardList[account].lastStakeDate != 0) {
            if (time > stakedRewardList[account].lastStakeDate) {
                rewardSum += (time - stakedRewardList[account].lastStakeDate) * _calcRewardPerSecond(stakedRewardList[account].staked);
            }
        }

        if (rewardSum > rewardMax) return rewardMax;
        return rewardSum;
    }

 
}

