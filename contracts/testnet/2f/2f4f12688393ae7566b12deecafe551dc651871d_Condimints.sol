pragma solidity ^0.8.11;

import "@openzeppelin/contracts/utils/Base64.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./chiru/ERC721ABurnable.sol";
import "forge-std/console.sol";

contract Condimints is ERC721ABurnable, Ownable {
    using Strings for uint256;

    struct MintDetails {
        uint256 meatlistStartTime;
        uint256 maxPerWalletMeatlist;
        uint256 maxMeatlistSupply;
        uint256 meatlistCount;

        uint256 publicStartTime;
        uint256 maxPerTx;
        uint256 price;
        uint256 maxSupply;
        uint256 totalSupply;
    }

    struct UserMintDetails {
        bool isMeatlist;
        uint256 meatlistMinted;
    }

    string public imgURI = "ipfs://QmT4Uty4r7aWw65fwcXirdbUGKbmjrLjdyWh8F5NZB2Xxh";
    bytes4 private constant _ERC2981_INTERFACE_ID = 0x2a55205a;
    uint256 private contractRoyalties = 690; // 6.9%
    address private royaltyReceiver = 0x2F5a2Ffd54194f99da6C75d43213f5D8DC5DBf03;

    uint256 public maxSupply = 5000;
	uint256 public maxMeatlistSupply = 2500;
    uint256 public maxPerTx = 5;
    uint256 public maxPerWalletMeatlist = 2;
    uint256 public price = 2 ether;
	uint256 public meatlistCount = 0;

    address payable private mintProceeds = payable(0xA00Deb25bD18F8Fe90427D78c9FdD6B89080401a);

    mapping(address => uint256) public meatlistMinted;
    mapping(address => bool) public meatlist;

    uint256 public meatlistStartTime = 1682784000;
    uint256 public publicStartTime = 1682785800;

    modifier mintCompliance(uint256 amount) {
        require(amount > 0, "Must mint one!");
        require(amount <= maxPerTx, "Too many in TX");
        require(maxSupply >= totalSupply() + amount, "None left");

        uint256 startTime = publicStartTime;
        if (meatlist[msg.sender] && block.timestamp < publicStartTime ) {
            startTime = meatlistStartTime;
            require(meatlistMinted[msg.sender] + amount <= maxPerWalletMeatlist, "Used all your meat");
			require(maxMeatlistSupply >= meatlistCount + amount, "Wait for public!");
        }
        require(block.timestamp >= startTime, "Can't mint yet!");
        require(msg.value >= price * amount, "Didn't pay enough");
        _;
    }

    constructor() ERC721A("Condimint", "CONDIM") {}

    function mintDetails() public view returns (MintDetails memory) {
        return MintDetails({
            meatlistStartTime: meatlistStartTime,
            maxPerWalletMeatlist: maxPerWalletMeatlist,
            maxMeatlistSupply: maxMeatlistSupply,
            meatlistCount: meatlistCount,
            publicStartTime: publicStartTime,
            maxPerTx: maxPerTx,
            price: price,
            maxSupply: maxSupply,
            totalSupply: totalSupply()
        });
    }

    function userMintDetails(address _user) public view returns (UserMintDetails memory) {
        return UserMintDetails({
            isMeatlist: meatlist[_user],
            meatlistMinted: meatlistMinted[_user]
        });
    }

    function mint(uint256 _quantity) external payable mintCompliance(_quantity) {
        if (meatlist[msg.sender]) {
            meatlistMinted[msg.sender] += _quantity;
			meatlistCount += _quantity;
        }

        (bool success, ) = mintProceeds.call{value: msg.value}("");
        require(success, "Failed to transfer mint proceeds");
        _mint(msg.sender, _quantity);
    }

    function adminMint(uint256 _quantity) external onlyOwner {
        require(_quantity > 0, "Must mint one!");
        require(maxSupply >= totalSupply() + _quantity, "None left");
        _mint(msg.sender, _quantity);
    }

    function addToMeatlist(address[] calldata meaters) external onlyOwner {
        for (uint i = 0; i < meaters.length; ++i) {
            meatlist[meaters[i]] = true;
        }
    }

    function setImgURI(string memory _imgURI) public onlyOwner {
        imgURI = _imgURI;
    }

    function setMaxSupply(uint256 _maxSupply) external onlyOwner {
        maxSupply = _maxSupply;
    }

	function setMeatlistStartTime(uint256 _meatlistStartTime) external onlyOwner {
		meatlistStartTime = _meatlistStartTime;
	}

	function setPublicStartTime(uint256 _publicStartTime) external onlyOwner {
		publicStartTime = _publicStartTime;
	}

    function setTokenRoyalties(uint96 _royalties) external onlyOwner {
        contractRoyalties = _royalties;
    }

    function setRoyaltyPayoutAddress(address _payoutAddress) external onlyOwner {
        royaltyReceiver = _payoutAddress;
    }

    function _startTokenId() internal view virtual override returns (uint256) {
        return 1;
    }

    /**
     * generates a base64 encoded metadata response without referencing off-chain content
     * @param tokenId the ID of the token to generate the metadata for
     * @return a base64 encoded JSON dictionary of the token's metadata and SVG
     */
    function tokenURI(uint256 tokenId) public view override(ERC721A, IERC721A) returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");

        string memory metadata = string(
            abi.encodePacked(
                '{"name": "Condimint #',
                tokenId.toString(),
                '", "description": "A mysterious bottle of unknown contents. Condimints may be burned 1:1 to produce Sausagers.", "image": "',
                imgURI,
                '", "attributes": [{"trait_type": "Generation", "value": "1"}, {"trait_type": "Artist", "value": "',
                unicode"Furkan Özçoban",
                '"}]}'
            )
        );

        return string(abi.encodePacked("data:application/json;base64,", Base64.encode(bytes(metadata))));
    }

    function royaltyInfo(
        uint256 /* _tokenId */,
        uint256 _salePrice
    ) external view returns (address receiver, uint256 royaltyAmount) {
        return (royaltyReceiver, ((_salePrice * contractRoyalties) / 10000));
    }

    function supportsInterface(bytes4 interfaceId) public view override(ERC721A, IERC721A) returns (bool) {
        if (interfaceId == _ERC2981_INTERFACE_ID) {
            return true;
        }

        return super.supportsInterface(interfaceId);
    }
}

