/// Roulette; supports a variety of bet types, spin the wheel to pick a number from 1-36.
contract Roulette is CentralGameBase, GameBet, GameChainlink {
    /// Many types of bets are supported, disambiguated by this enum.
    /// Requires a betValue to know exactly what kind of bet is being placed.
    enum BetType {
        /// The exact number.
        NUMBER,
        /// Red (0) or black (1)
        RED_BLACK,
        /// Odd (0) or even (1).
        ODD_EVEN,
        /// 1-12 (0) or 13-24 (1) or 15-36 (2).
        THIRDS,
        /// 1-18 (0) or 9-36 (1).
        HALVES,
        /// 1-34 % 3 (0) or 2-35 % 3 (1) or 3-36 % 3 (2).
        ROWS
    }

    /// An individual bet placed by the user.
    /// A game is composed of an array of bets, which don't need to be unique.
    struct Bet {
        uint256 betAmount;
        BetType betType;
        uint8 betValue;
    }

    /// Per-round result object for this game.
    struct RoundInfo {
        RoundState state;
        uint256 sent;
        uint8 selected;
    }

    event GameStarted(
        uint256 indexed gameId,
        address indexed creator,
        uint8 subgame,
        uint8 numRounds,
        uint256 betPerRound,
        uint256 rakePerRound,
        Bet[] choices
    );
    event WinnerPicked(uint256 indexed gameId, uint8 subgame, RoundInfo[] results);

    /// Per-game result payouts, by gameId.
    mapping(uint256 => uint256[37]) public payoutsByGameId;

    /// Contract can be funded.
    receive() external payable {}

    /// Constructor populates generic Chainlink values.
    constructor(
        address vrfCoordinatorV2,
        bytes32 gasLane,
        uint64 subscriptionId,
        uint32 callbackGasLimit
    ) GameChainlink(vrfCoordinatorV2, gasLane, subscriptionId, callbackGasLimit) {}

    /// Create a new game.
    function playGame(
        uint8 numRounds,
        Bet[] memory userChoices
    ) external payable isEOA isNotPaused returns (uint256 gameId) {
        uint256 totalBet;
        uint256[37] memory payouts;

        for (uint8 i; i < userChoices.length; ++i) {
            Bet memory bet = userChoices[i];
            require(bet.betAmount > 0, "can't place a bet without an amount");
            totalBet += bet.betAmount;
            appendToPayouts(bet, payouts);
        }

        uint256 maxPayout;
        for (uint8 i; i < 37; ++i) {
            uint256 p = payouts[i];
            if (p > maxPayout) {
                maxPayout = p;
            }
        }

        IRakeDistributor rakeDistributor = IRakeDistributor(rakeDistributorAddress);
        BetDetails memory details = validateBetWithMaxPayoutValue(
            treasuryAddress.balance,
            rakeDistributor.getTotalRake(),
            numRounds,
            maxPayout
        );

        require(totalBet == details.bet, "tx value did not match bet value");

        currentGameId++;
        GameInfo memory fi;
        fi.gameId = currentGameId;
        fi.betPerRound = details.betPerRound;
        fi.rakePerRound = details.rakePerRound;
        fi.creator = msg.sender;
        // Not providing referrer yet.
        fi.subgame = 9;
        fi.numRounds = numRounds;
        fi.state = GameState.CALCULATING;

        (bool success,) = payable(treasuryAddress).call{value: details.bet}("");
        require(success, "Failed to send to treasury");
        rakeDistributor.distributeRake{value: details.rake}();

        payoutsByGameId[fi.gameId] = payouts;
        gameById[fi.gameId] = fi;
        _requestGameFulfillment(fi.gameId);

        // Synthetically adjust the game counter forward by the number of rounds.
        // This leaves gaps in the mapping, but it's useful for the offline storage.
        currentGameId += numRounds - 1;

        emit GameStarted(
            fi.gameId,
            msg.sender,
            fi.subgame,
            numRounds,
            fi.betPerRound,
            fi.rakePerRound,
            userChoices
        );
        return fi.gameId;
    }

    /// Receives the random number from VRF and calculates win/loss/tie.
    /// Transfers the appropriate amount of funds from treasury to the user on wins.
    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
        uint256 gameId = gameIdByRequestId[requestId];

        GameInfo storage fi = gameById[gameId];
        uint256[37] memory payouts = payoutsByGameId[gameId];
        uint8 numRounds = fi.numRounds;
//        uint256 betPerRound = fi.betPerRound;

        RoundInfo[] memory results = new RoundInfo[](numRounds);
        uint256 amountToUser = 0;

        for (uint i; i < numRounds; ++i) {
            uint256 roundRandom = uint256(keccak256(abi.encode(randomWords[0], i)));
            uint8 roundValue = uint8(roundRandom % 37);
            uint256 payout = payouts[roundValue];
            RoundInfo memory result = results[i];
            result.selected = roundValue;

            // Any payout in roulette is a win since all multipliers are greater than 1.
            if (payout > 0) {
                result.state = RoundState.WON;
                result.sent = payout;
                amountToUser += payout;
            } else {
                result.state = RoundState.LOST;
            }
        }

        if (amountToUser > 0) {
            ITreasury treasury = ITreasury(treasuryAddress);
            treasury.sendPayout(fi.creator, amountToUser);
        }

        fi.state = GameState.CLOSED;
        emit WinnerPicked(gameId, fi.subgame, results);
    }

    /// Triggers VRF process for a given game in case of emergency.
    function forceRequestWinner(uint256 gameId) external onlyOwner {
        GameInfo storage fi = gameById[gameId];
        require(fi.state != GameState.CLOSED, "already closed");
        _requestGameFulfillment(fi.gameId);
    }

    function appendToPayouts(Bet memory bet, uint256[37] memory payouts) internal {
        if (bet.betType == BetType.NUMBER) {
            require(bet.betValue < 37, "numbers must be from 0-36");
            payouts[bet.betValue] += bet.betAmount * 36;
        } else if (bet.betType == BetType.RED_BLACK) {
            require(bet.betValue < 2, "red/black bet value must be 0 or 1");
            uint256 twoTimesBet = bet.betAmount * 2;
            for (uint8 j = 1 + bet.betValue; j < 11; j = j += 2) {
                payouts[j] += twoTimesBet;
            }
            for (uint8 j = 11 + bet.betValue; j < 19; j = j += 2) {
                payouts[j] += twoTimesBet;
            }
            for (uint8 j = 19 + bet.betValue; j < 29; j = j += 2) {
                payouts[j] += twoTimesBet;
            }
            for (uint8 j = 29 + bet.betValue; j < 37; j = j += 2) {
                payouts[j] += twoTimesBet;
            }
        } else if (bet.betType == BetType.ODD_EVEN) {
            require(bet.betValue < 2, "odd/even bet value must be 0 or 1");
            uint256 twoTimesBet = bet.betAmount * 2;
            for (uint8 j = 1 + bet.betValue; j < 37; j += 2) {
                payouts[j] += twoTimesBet;
            }
        } else if (bet.betType == BetType.THIRDS) {
            require(bet.betValue < 3, "thirds bet value must be 0 or 1 or 2");
            uint8 start = 1 + 12 * bet.betValue;
            uint8 stop = start + 12;
            uint256 threeTimesBet = bet.betAmount * 3;
            for (uint8 j = start; j < stop; ++j) {
                payouts[j] += threeTimesBet;
            }
        } else if (bet.betType == BetType.HALVES) {
            require(bet.betValue < 2, "halves bet value must be 0 or 1");
            uint8 start = 1 + 18 * bet.betValue;
            uint8 stop = start + 18;
            uint256 twoTimesBet = bet.betAmount * 2;
            for (uint8 j = start; j < stop; ++j) {
                payouts[j] += twoTimesBet;
            }
        } else if (bet.betType == BetType.ROWS) {
            require(bet.betValue < 3, "rows bet value must be 0 or 1 or 2");
            uint8 start = 1 + 18 * bet.betValue;
            uint8 stop = start + 18;
            uint256 threeTimesBet = bet.betAmount * 3;
            for (uint8 j = start; j < stop; j += 3) {
                payouts[j] += threeTimesBet;
            }
        } else {
            revert("internal error: unexpected bet type");
        }
    }
}

