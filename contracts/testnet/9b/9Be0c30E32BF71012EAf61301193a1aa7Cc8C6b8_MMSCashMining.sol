        if (xSignifier & 0x8 > 0) resultSignifier = resultSignifier * 0x100000000000000000000000000000004 >> 128;
        if (xSignifier & 0x4 > 0) resultSignifier = resultSignifier * 0x100000000000000000000000000000001 >> 128;

        if (!xNegative) {
          resultSignifier = resultSignifier >> 15 & 0xFFFFFFFFFFFFFFFFFFFFFFFFFFFF;
          resultExponent += 0x3FFF;
        } else if (resultExponent <= 0x3FFE) {
          resultSignifier = resultSignifier >> 15 & 0xFFFFFFFFFFFFFFFFFFFFFFFFFFFF;
          resultExponent = 0x3FFF - resultExponent;
        } else {
          resultSignifier = resultSignifier >> resultExponent - 16367;
          resultExponent = 0;
        }

        return bytes16 (uint128 (resultExponent << 112 | resultSignifier));
      }
    }
  }

  /**
   * Calculate e^x.
   *
   * @param x quadruple precision number
   * @return quadruple precision number
   */
  function exp (bytes16 x) internal pure returns (bytes16) {
    unchecked {
      return pow_2 (mul (x, 0x3FFF71547652B82FE1777D0FFDA0D23A));
    }
  }

  /**
   * Get index of the most significant non-zero bit in binary representation of
   * x.  Reverts if x is zero.
   *
   * @return index of the most significant non-zero bit in binary representation
   *         of x
   */
  function mostSignificantBit (uint256 x) private pure returns (uint256) {
    unchecked {
      require (x > 0);

      uint256 result = 0;

      if (x >= 0x100000000000000000000000000000000) { x >>= 128; result += 128; }
      if (x >= 0x10000000000000000) { x >>= 64; result += 64; }
      if (x >= 0x100000000) { x >>= 32; result += 32; }
      if (x >= 0x10000) { x >>= 16; result += 16; }
      if (x >= 0x100) { x >>= 8; result += 8; }
      if (x >= 0x10) { x >>= 4; result += 4; }
      if (x >= 0x4) { x >>= 2; result += 2; }
      if (x >= 0x2) result += 1; // No need to shift x anymore

      return result;
    }
  }
}


library SafeMath {

  /**
  * @dev Multiplies two numbers, throws on overflow.
  */
  function mul(uint256 a, uint256 b) internal pure returns (uint256 c) {
    if (a == 0) {
      return 0;
    }
    c = a * b;
    assert(c / a == b);
    return c;
  }

  /**
  * @dev Integer division of two numbers, truncating the quotient.
  */
  function div(uint256 a, uint256 b) internal pure returns (uint256) {
    // assert(b > 0); // Solidity automatically throws when dividing by 0
    // uint256 c = a / b;
    // assert(a == b * c + a % b); // There is no case in which this doesn't hold
    return a / b;
  }

  /**
  * @dev Subtracts two numbers, throws on overflow (i.e. if subtrahend is greater than minuend).
  */
  function sub(uint256 a, uint256 b) internal pure returns (uint256) {
    assert(b <= a);
    return a - b;
  }

  /**
  * @dev Adds two numbers, throws on overflow.
  */
  function add(uint256 a, uint256 b) internal pure returns (uint256 c) {
    c = a + b;
    assert(c >= a);
    return c;
  }
}

contract MMSCashMining is ReentrancyGuard, Ownable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    
    bool public started = true;
    bool private wStarted = true;
    bool private cStarted = true;
    IERC20 public token = IERC20(0xd24739f4F877e12Cbc43Ac9a0F3EA73c20019337);
    uint256 public latestOrderId;
    uint256 public latestPlanId;
    uint256 public compoundTime = 30;
    uint256[5] private  MAX_STAKE = [0,777000000000000000000000000,7777000000000000000000000000,77770000000000000000000000000,777777000000000000000000000000];
    uint256[5] private RATES=[10000,200,150,100,50];
    uint256[5] private TIME=[0,3600,3600,3600,3600];
    uint256[5] private MIN=[0,1000000000000000000000,1000000000000000000000,1000000000000000000000,1000000000000000000000];
    uint256[5] private TOTAL_STAKE=[0,0,0,0,0];
    
    struct OrderInfo {
        address beneficiary;
        uint256 planId;
        uint256 amount;
        uint256 time;
        uint256 rate;
        uint256 starttime;
        uint256 endtime;
        uint256 claimedReward;
        bool claimed;
    }

    
    uint256 public totalStake;
    uint256 public totalWithdrawal;
    uint256 public totalRewardDistributed;
    uint256 public totalRewardPending;
    mapping(address => uint256) public totalRewardEarn;
    mapping(address => uint256) public balanceOf;
    mapping(uint256 => OrderInfo) public orders;
    mapping(address => uint256[]) private orderIds;
    mapping(address => bool) public isUserWhitelisted;

    
    

    constructor() {
        
    }

    event Deposit(address indexed user, uint256 indexed lockupDuration, uint256 amount , uint256 returnPer);
    event Withdraw(address indexed user, uint256 amount , uint256 reward , uint256 total );
    event WithdrawAll(address indexed user, uint256 amount, uint256 penlty , uint256 total);
    event Claimed(address indexed user, uint256 amount, uint256 time);

    function getActivePlanId() public view returns(uint){
        uint value;
        for(uint i=0;i<5;i++){
            if(MAX_STAKE[i] > TOTAL_STAKE[i]){
                value = i;
                break;
            }
        }

        return value;
    }

    function getPoolInfo(uint _id) public view returns(uint256,uint256,uint256,uint256,uint256){
      return (MAX_STAKE[_id] , RATES[_id] , TIME[_id] , MIN[_id] , TOTAL_STAKE[_id]);
    }

    function deposit(uint planId , uint256 _amount) public {
        require(started , "right now deposit stop , try again later");
        require(address(token) != address(0), "Token Not Set Yet");
        require(planId <= 4, "Invalid planId found");
        if(planId != 0){
          uint activePlanId = getActivePlanId();
          require(planId == activePlanId, "Plan not active right now");
          uint total_left_stake = MAX_STAKE[planId].sub(TOTAL_STAKE[planId]);
          require(_amount >= MIN[planId] || total_left_stake < MIN[planId], "amount limit failed, please check and try again!");
          require(_amount <= total_left_stake, "please check left token amount limit and try again!");
        }
        else{
          require(isUserWhitelisted[msg.sender] , "address not allowed to stake on pool.");
        }
        
        require(token.transferFrom(msg.sender, address(this), _amount), "Transfer failed");
        uint256 userReward = _amount.sub(accurateInterest(_amount , RATES[planId].mul(10**14) , TIME[planId]));

        orders[++latestOrderId] = OrderInfo(
             msg.sender,
             planId,
            _amount,
            TIME[planId],
            RATES[planId],
            block.timestamp,
            planId == 0 ? 0 : block.timestamp.add(TIME[planId]),
            0,
            false
        );

        TOTAL_STAKE[planId] = TOTAL_STAKE[planId].add(_amount);
        totalStake = totalStake.add(_amount);
        totalRewardPending = totalRewardPending.add(userReward);
        balanceOf[msg.sender] = balanceOf[msg.sender].add(_amount);
        orderIds[msg.sender].push(latestOrderId);

        emit Deposit(msg.sender , TIME[planId] , _amount , RATES[planId] );
    }
    
    function withdraw(uint256 orderId) public nonReentrant{
        require(wStarted , "right now withdraw not avalible, try again later");
        require(orderId <= latestOrderId, "the order ID is incorrect"); // IOI
        OrderInfo storage orderInfo = orders[orderId];
        require(msg.sender == orderInfo.beneficiary, "not order beneficiary"); // NOO
        require(balanceOf[msg.sender] >= orderInfo.amount, "insufficient redeemable tokens"); // ITA
        require(block.timestamp >= orderInfo.endtime || orderInfo.planId == 0,"tokens are being locked"); // TIL
        require(!orderInfo.claimed, "tokens already claimed"); // TAC
        
        uint256 amount =  orderInfo.amount;
        uint reward;
        if(orderInfo.planId == 0){
          uint countSec = block.timestamp.sub(orderInfo.starttime);
          reward = amount.sub(accurateInterest(amount , orderInfo.rate.mul(10**14) , countSec));
        }
        else{
          reward = amount.sub(accurateInterest(amount , orderInfo.rate.mul(10**14) , orderInfo.time));
        }
         
        uint256 claimAvailable =  reward.sub(orderInfo.claimedReward);
        uint256 total = amount.add(claimAvailable);
        
        require(token.balanceOf(address(this)) >= total, "Currently Withdraw not Avalible");
        
        totalRewardEarn[msg.sender] = totalRewardEarn[msg.sender].add(claimAvailable);
        totalWithdrawal = totalWithdrawal.add(amount);
        totalRewardDistributed = totalRewardDistributed.add(claimAvailable);
        totalRewardPending = totalRewardPending.sub(reward);
        orderInfo.claimed = true;
        balanceOf[msg.sender] = balanceOf[msg.sender].sub(amount);
        token.transfer(address(msg.sender) , total);
        emit Withdraw(msg.sender , amount , claimAvailable , total);
    }


    function userPendingReward(uint256 orderId) public view returns(uint){
        uint256 totalAvalible;
        
        if(orderId <= latestOrderId){
            OrderInfo storage orderInfo = orders[orderId];
            if(!orderInfo.claimed){
                if(block.timestamp >= orderInfo.endtime){
                    uint reward = orderInfo.amount.sub(accurateInterest(orderInfo.amount , orderInfo.rate.mul(10**14) , orderInfo.time));
                    totalAvalible =  reward.sub(orderInfo.claimedReward);
                }
                else{
                    uint256 totalSec = block.timestamp.sub(orderInfo.starttime);
                    uint reward = orderInfo.amount.sub(accurateInterest(orderInfo.amount , orderInfo.rate.mul(10**14) , totalSec));
                    totalAvalible =  reward.sub(orderInfo.claimedReward);
                }
            }
        }
        
        return totalAvalible;
    }

    function claimReward(uint256 orderId)  public nonReentrant{
        require(cStarted , "right now withdraw not avalible, try again later");
        require(orderId <= latestOrderId, "the order ID is incorrect"); // IOI
        OrderInfo storage orderInfo = orders[orderId];
        require(msg.sender == orderInfo.beneficiary, "not order beneficiary"); // NOO
        require(balanceOf[msg.sender] >= orderInfo.amount, "insufficient redeemable tokens"); // ITA
        require(!orderInfo.claimed, "tokens are ready to be claimed"); // TAC
        uint256 totalAvalible;
        uint reward;
        if(block.timestamp >= orderInfo.endtime){
            reward = orderInfo.amount.sub(accurateInterest(orderInfo.amount , orderInfo.rate.mul(10**14) , orderInfo.time));
            totalAvalible =  reward.sub(orderInfo.claimedReward);
        }
        else{
            uint256 totalSec = block.timestamp.sub(orderInfo.starttime);
            reward = orderInfo.amount.sub(accurateInterest(orderInfo.amount , orderInfo.rate.mul(10**14) , totalSec));
            totalAvalible =  reward.sub(orderInfo.claimedReward);
        }

        require(totalAvalible > 0 , "no pending reward found");
        require(token.balanceOf(address(this)) >= totalAvalible , "right now reward claim not avalible , try again later");
        orderInfo.claimedReward = reward;
        token.transfer(address(msg.sender), totalAvalible);

        emit Claimed(address(msg.sender) , totalAvalible , block.timestamp);
    }


    function pow(int128 _x, uint256 _n) public pure returns (int128 r) {
        r = ABDKMath64x64.fromUInt(1);
        while (_n > 0) {
            if (_n % 2 == 1) {
                r = ABDKMath64x64.mul(r, _x);
                _n -= 1;
            } else {
                _x = ABDKMath64x64.mul(_x, _x);
                _n /= 2;
            }
        }
    }

    function accurateInterest(uint256  currentBalance , uint256 ratio , uint256 _time ) public view returns(uint){
        return compound(currentBalance, ratio, _time);
    }

   
    function compound(
        uint256 _principal,
        uint256 _ratio,
        uint256 _n
    ) public view returns (uint256) {
        uint256 daysCount = _n.div(86400*compoundTime);

        return
            ABDKMath64x64.mulu(
                pow(ABDKMath64x64.add(ABDKMath64x64.fromUInt(1), ABDKMath64x64.divu(_ratio, 10**18)), daysCount),
                _principal
            );
    }


    function withdrawBNB(address payable _reciever, uint256 _amount)
        public
        onlyOwner
    {
        _reciever.transfer(_amount);
    }


    function transferERC20Token(
        address tokenAddress,
        uint256 tokens
    ) public onlyOwner {
        IERC20(tokenAddress).transfer(address(msg.sender), tokens);
    }

    function setToken(address _token) public onlyOwner{
      token = IERC20(_token);
    }

    function setStarted(bool _isactive) public onlyOwner{
      started = _isactive;
    }

    function setWStarted(bool _isactive) public onlyOwner{
      wStarted = _isactive;
    }

    function setCStarted(bool _isactive) public onlyOwner{
      cStarted = _isactive;
    }
    
    function addUsersWhitelisted(address[] memory _addresss) public onlyOwner{
      for(uint i=0;i<_addresss.length;i++){
        isUserWhitelisted[_addresss[i]] = true;
      }
    }

    function removeUsersWhitelisted(address[] memory _addresss) public onlyOwner{
      for(uint i=0;i<_addresss.length;i++){
        isUserWhitelisted[_addresss[i]] = false;
      }
    }

    function setMaxStake(uint256[4] memory _maxstake) public onlyOwner{
        MAX_STAKE = [0,_maxstake[0],_maxstake[1],_maxstake[2],_maxstake[3]];
    }

    function setMinStake(uint256[4] memory _minstake) public onlyOwner{
        MIN = [0,_minstake[0],_minstake[1],_minstake[2],_minstake[3]];
    }

    function setStakeTime(uint256[4] memory _staketime) public onlyOwner{
        TIME = [0,_staketime[0],_staketime[1],_staketime[2],_staketime[3]];
    }

    function setStakeRates(uint256[4] memory _rates) public onlyOwner{
        RATES = [0,_rates[0],_rates[1],_rates[2],_rates[3]];
    }
}