    UUPSUpgradeable
{
    using AddressUpgradeable for address;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(address admin_) public initializer {
        __AccessControlEnumerable_init();
        __UUPSUpgradeable_init();
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(DEFAULT_ADMIN_ROLE, admin_);
        _grantRole(LibPT.DEPLOYER_ROLE, admin_);
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyRole(LibPT.DEPLOYER_ROLE)
    {}

    function onERC721Transferred(
        address from,
        address to,
        address contractAddress,
        uint256 tokenId
    ) external {
        emit LibPT.NFTBalanceChanged(contractAddress, from, tokenId, -1);
        emit LibPT.NFTBalanceChanged(contractAddress, to, tokenId, 1);
    }

    function onERC721Received(
        address operator,
        address from,
        uint256 tokenId,
        bytes calldata data
    ) external returns (bytes4) {
        address to = msg.sender;
