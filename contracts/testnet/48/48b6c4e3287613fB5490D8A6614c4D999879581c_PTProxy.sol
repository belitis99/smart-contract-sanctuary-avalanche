    using CountersUpgradeable for CountersUpgradeable.Counter;
    CountersUpgradeable.Counter private _tokenIdCounter;
    string public baseURI;
    uint256 public limit;
    bool public transferEnabled;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(
        string calldata name_,
        string calldata symbol_,
        string calldata baseURI_,
        uint256 limit_
    ) public initializer {
        __ERC721_init(name_, symbol_);
        __ERC721Enumerable_init();
        __Pausable_init();
        __AccessControlEnumerable_init();
        __ERC721Burnable_init();
        __UUPSUpgradeable_init();
        __ReentrancyGuard_init();

        baseURI = baseURI_;
        limit = limit_;
        transferEnabled = true;

        _grantRole(DEFAULT_ADMIN_ROLE, _msgSender());
    }

    function _baseURI() internal view override returns (string memory) {
        return baseURI;
    }

    function pause() public onlyRole(LibPT.DEPLOYER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(LibPT.DEPLOYER_ROLE) {
        _unpause();
    }

    function toggleTransferEnabled(bool canTransfer_)
        external
        onlyRole(LibPT.DEPLOYER_ROLE)
    {
        transferEnabled = canTransfer_;
    }

    function safeMint(address to)
        public
        onlyRole(LibPT.PLATFORM_ROLE)
        whenNotPaused
    {
        require(
            balanceOf(to) < limit,
            "ERC721Campaign: balance of owner reached the limit."
        );
        uint256 tokenId = _tokenIdCounter.current();
        _tokenIdCounter.increment();
        _safeMint(to, tokenId);
        // _mint(to, tokenId);
    }

    function multSafeMint(address[] calldata recipients)
        external
        onlyRole(LibPT.PLATFORM_ROLE)
        whenNotPaused
        nonReentrant
    {
        for (uint8 i = 0; i < recipients.length; ) {
            safeMint(recipients[i]);
            unchecked {
                ++i;
            }
        }
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 firstTokenId,
        uint256 batchSize
    )
        internal
        override(ERC721Upgradeable, ERC721EnumerableUpgradeable)
        whenNotPaused
    {
        require(
            balanceOf(to) < limit,
            "ERC721Campaign: balance of owner reached the limit."
        );
        super._beforeTokenTransfer(from, to, firstTokenId, batchSize);
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyRole(LibPT.DEPLOYER_ROLE)
    {}

