        _accessControl = accessControl_;
        _eventLogger = eventLogger_;
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(AccessControlEnumerableUpgradeable, IERC165Upgradeable)
        returns (bool)
    {
        return
            interfaceId == type(IAccount).interfaceId ||
            super.supportsInterface(interfaceId);
    }

    function getRoleMember(bytes32 role, uint256 index)
        public
        view
        virtual
        override
        returns (address)
    {
        return
            AccessControlEnumerableUpgradeable(_accessControl).getRoleMember(
                role,
                index
            );
    }

    function getRoleMemberCount(bytes32 role)
        public
        view
        virtual
        override
        returns (uint256)
    {
        return
            AccessControlEnumerableUpgradeable(_accessControl)
                .getRoleMemberCount(role);
    }

    function hasRole(bytes32 role, address account)
        public
        view
        virtual
        override(AccessControlUpgradeable, IAccessControlUpgradeable)
        returns (bool)
    {
        return AccessControlUpgradeable(_accessControl).hasRole(role, account);
    }

    function getRoleAdmin(bytes32 role)
        public
        view
        virtual
        override(AccessControlUpgradeable, IAccessControlUpgradeable)
        returns (bytes32)
    {
