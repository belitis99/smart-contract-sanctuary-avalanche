pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/ClonesUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "./PTEventLogger.sol";
import "./LibPT.sol";

contract PTProxy is
    PausableUpgradeable,
    AccessControlEnumerableUpgradeable,
    UUPSUpgradeable,
    ReentrancyGuardUpgradeable
{
    using AddressUpgradeable for address;
    address private _accessControl;
    address private _eventLogger;

    event AccountCreated(address addressCreated, bytes32 salt);

    event Minted(
        address indexed contractAddress,
        bool indexed success,
        bytes32 indexed data
    );

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(
        address admin_,
        address accessControl_,
        address eventLogger_
    ) public initializer {
        __UUPSUpgradeable_init();
        __ReentrancyGuard_init();
        __Pausable_init();
        __AccessControlEnumerable_init();

        _accessControl = accessControl_;
        _eventLogger = eventLogger_;

        _grantRole(DEFAULT_ADMIN_ROLE, admin_);
        _grantRole(LibPT.PLATFORM_ROLE, admin_);
        _grantRole(LibPT.DEPLOYER_ROLE, admin_);
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(LibPT.PLATFORM_ROLE, msg.sender);
        _grantRole(LibPT.DEPLOYER_ROLE, msg.sender);
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyRole(LibPT.DEPLOYER_ROLE)
    {}

    function pause() external onlyRole(LibPT.DEPLOYER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(LibPT.DEPLOYER_ROLE) {
        _unpause();
    }

    function setAccessControl(address accessControl_)
        public
        onlyRole(LibPT.DEPLOYER_ROLE)
    {
        _accessControl = accessControl_;
    }

    function setEventLogger(address eventLogger_)
        public
        onlyRole(LibPT.DEPLOYER_ROLE)
    {
        _eventLogger = eventLogger_;
    }

    function createAccount(
        address implementation,
        address accessControl_,
        address eventLogger_,
        bytes32 salt
    ) public onlyRole(LibPT.PLATFORM_ROLE) {
        address created = ClonesUpgradeable.predictDeterministicAddress(
            implementation,
            salt
        );

        if (AddressUpgradeable.isContract(created)) {
            emit AccountCreated(created, salt);
            return;
        } else {
            ClonesUpgradeable.cloneDeterministic(implementation, salt);
        }

        (bool createSuccess, ) = address(created).call(
            abi.encodeWithSelector(
                IAccount.initialize.selector,
                accessControl_,
                eventLogger_
            )
        );
        require(createSuccess, "PTProxy: create failed.");
        emit AccountCreated(created, salt);
    }

    function multiCreateAccount(
        address implementation,
        address accessControl_,
        address eventLogger_,
        bytes32[] calldata salts
    ) external onlyRole(LibPT.PLATFORM_ROLE) {
        for (uint8 i = 0; i < salts.length; ) {
            createAccount(
                implementation,
                accessControl_,
                eventLogger_,
                salts[i]
            );
            unchecked {
                ++i;
            }
        }
    }

    function getAddress(address implementation, bytes32 salt)
        external
        view
        returns (address)
    {
        return
            ClonesUpgradeable.predictDeterministicAddress(implementation, salt);
    }

    function multiCreateAccountAndMintTo(
        address implementation,
        address accessControl_,
        address eventLogger_,
        LibPT.CallMintWithSaltAndData[] calldata callMints
    ) external onlyRole(LibPT.PLATFORM_ROLE) {
        for (uint8 i = 0; i < callMints.length; ) {
            createAccountAndMintTo(
                implementation,
                accessControl_,
                eventLogger_,
                callMints[i].salt,
                callMints[i].contractAddress,
                callMints[i].data
            );
            unchecked {
                ++i;
            }
        }
    }

    function createAccountAndMintTo(
        address implementation,
        address accessControl_,
        address eventLogger_,
        bytes32 salt,
        address contractAddress,
        bytes32 data
    ) public onlyRole(LibPT.PLATFORM_ROLE) {
        address to = ClonesUpgradeable.predictDeterministicAddress(
            implementation,
            salt
        );
        if (!AddressUpgradeable.isContract(to)) {
            createAccount(implementation, accessControl_, eventLogger_, salt);
        }
        (bool mintSuccess, ) = address(contractAddress).call(
            abi.encodeWithSelector(IERC721.safeMint.selector, to)
        );
        emit Minted(contractAddress, mintSuccess, data);
        require(mintSuccess, "PTProxy: mint failed.");
    }

    function isAccount(address address_) public returns (bool) {
        (bool success, bytes memory data) = address(address_).call(
            abi.encodeWithSelector(
                IAccount.supportsInterface.selector,
                type(IAccount).interfaceId
            )
        );

        if (!success) {
            return false;
        }

        bool isAccount_ = abi.decode(data, (bool));
        return isAccount_;
    }

    function transferERC20(
        address from,
        address to,
        address contractAddress,
        uint256 amount
    ) external onlyRole(LibPT.PLATFORM_ROLE) {
        require(amount > 0, "PTProxy: amount should be more than zero.");
        require(
            from != address(0) &&
                to != address(0) &&
                contractAddress != address(0),
            "PTProxy: any address should not be zero address."
        );
        require(from != to, "PTProxy: 'to' should not be same as 'from'.");
        require(isAccount(from), "PTProxy: 'from' should be an account.");

        bytes memory data = abi.encodeWithSelector(
            IERC20.transfer.selector,
            from,
            to,
            amount
        );

        (bool transferSuccess, ) = address(from).call(
            abi.encodeWithSelector(
                IAccount.proxyCall.selector,
                contractAddress,
                data
            )
        );

        require(transferSuccess, "PTProxy: transfer failed.");
    }

    function transferERC721(
        address from,
        address to,
        address contractAddress,
        uint256 tokenId
    ) external onlyRole(LibPT.PLATFORM_ROLE) {
        require(
            from != address(0) &&
                to != address(0) &&
                contractAddress != address(0),
            "PTProxy: any address should not be zero address."
        );
        require(from != to, "PTProxy: 'to' should not be same as 'from'.");
        require(isAccount(from), "PTProxy: 'from' should be an account.");

        bytes memory data = abi.encodeWithSelector(
            IERC721.safeTransferFrom.selector,
            from,
            to,
            tokenId
        );

        (bool transferSuccess, ) = address(from).call(
            abi.encodeWithSelector(
                IAccount.proxyCall.selector,
                contractAddress,
                data
            )
        );

        require(transferSuccess, "PTProxy: transfer failed.");
        address(_eventLogger).call(
            abi.encodeWithSelector(
                PTEventLogger.onERC721Transferred.selector,
                from,
                to,
                contractAddress,
                tokenId
            )
        );
    }

    function transferERC1155(
        address from,
        address to,
        address contractAddress,
        uint256 tokenId,
        uint256 amount
    ) external onlyRole(LibPT.PLATFORM_ROLE) {
        require(amount > 0, "PTProxy: amount should be more than zero.");
        require(
            from != address(0) &&
                to != address(0) &&
                contractAddress != address(0),
            "PTProxy: any address should not be zero address."
        );
        require(from != to, "PTProxy: 'to' should not be same as 'from'.");
        require(isAccount(from), "PTProxy: 'from' should be an account.");

        bytes memory data = abi.encodeWithSelector(
            IERC1155.safeTransferFrom.selector,
            from,
            to,
            tokenId,
            amount,
            ""
        );

        (bool transferSuccess, ) = address(from).call(
            abi.encodeWithSelector(
                IAccount.proxyCall.selector,
                contractAddress,
                data
            )
        );

        require(transferSuccess, "PTProxy: transfer failed.");
        address(_eventLogger).call(
            abi.encodeWithSelector(
                PTEventLogger.onERC1155Transferred.selector,
                from,
                to,
                contractAddress,
                tokenId,
                amount
            )
        );
    }

    function getRoleMember(bytes32 role, uint256 index)
        public
        view
        virtual
        override
        returns (address)
    {
        return
            AccessControlEnumerableUpgradeable(_accessControl).getRoleMember(
                role,
                index
            );
    }

    function getRoleMemberCount(bytes32 role)
        public
        view
        virtual
        override
        returns (uint256)
    {
        return
            AccessControlEnumerableUpgradeable(_accessControl)
                .getRoleMemberCount(role);
    }

    function hasRole(bytes32 role, address account)
        public
        view
        virtual
        override(AccessControlUpgradeable, IAccessControlUpgradeable)
        returns (bool)
    {
        return AccessControlUpgradeable(_accessControl).hasRole(role, account);
    }

    function getRoleAdmin(bytes32 role)
        public
        view
        virtual
        override(AccessControlUpgradeable, IAccessControlUpgradeable)
        returns (bytes32)
    {
        return AccessControlUpgradeable(_accessControl).getRoleAdmin(role);
    }
}