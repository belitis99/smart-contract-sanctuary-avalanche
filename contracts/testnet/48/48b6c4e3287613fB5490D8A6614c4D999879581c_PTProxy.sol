        emit LibPT.NFTBalanceChanged(operator, to, tokenId, 1);
        return IERC721ReceiverUpgradeable.onERC721Received.selector;
    }

    function onERC1155Transferred(
        address from,
        address to,
        address contractAddress,
        uint256 tokenId,
        uint256 value
    ) external {
        emit LibPT.NFTBalanceChanged(
            contractAddress,
            from,
            tokenId,
            -1 * int256(value)
        );
        emit LibPT.NFTBalanceChanged(
            contractAddress,
            to,
            tokenId,
            int256(value)
        );
    }

    function onERC1155BatchTransferred(
