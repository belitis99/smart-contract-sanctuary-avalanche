    ) external {
        for (uint256 i = 0; i < ids.length; ) {
            emit LibPT.NFTBalanceChanged(
                contractAddress,
                from,
                ids[i],
                -1 * int256(values[i])
            );
            emit LibPT.NFTBalanceChanged(
                contractAddress,
                to,
                ids[i],
                int256(values[i])
            );
            unchecked {
                ++i;
            }
        }
    }

    function onERC1155Received(
