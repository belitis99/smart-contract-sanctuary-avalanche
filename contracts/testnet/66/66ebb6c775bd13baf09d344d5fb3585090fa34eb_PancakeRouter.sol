    // **** SWAP ****
    // requires the initial amount to have already been sent to the first pair
    function _swap(
        uint256[] memory amounts,
        address[] memory path,
        address _to
    ) internal virtual {
        for (uint256 i; i < path.length - 1; i++) {
            (address input, address output) = (path[i], path[i + 1]);
            (address token0, ) = PancakeLibrary.sortTokens(input, output);
            uint256 amountOut = amounts[i + 1];
            (uint256 amount0Out, uint256 amount1Out) = input == token0
                ? (uint256(0), amountOut)
                : (amountOut, uint256(0));
            address to = i < path.length - 2
                ? PancakeLibrary.pairFor(factory, output, path[i + 2])
                : _to;
            IPancakePair(PancakeLibrary.pairFor(factory, input, output)).swap(
                amount0Out,
                amount1Out,
                to,
                new bytes(0)
            );
        }
    }

    function _swapExactTokensForTokens(
        uint256 amountIn,
        uint256 amountOutMin,
        address[] memory path,
        address to,
        uint256 deadline,
        address funAddress
    ) private ensure(deadline) returns (uint256[] memory amounts) {
        amounts = PancakeLibrary.getAmountsOut(factory, amountIn, path);
        require(
            amounts[amounts.length - 1] >= amountOutMin,
            "PancakeRouter: INSUFFICIENT_OUTPUT_AMOUNT"
        );

        TransferHelper.safeTransferFrom(
            path[0],
            funAddress,
            PancakeLibrary.pairFor(factory, path[0], path[1]),
            amounts[0]
        );

        _swap(amounts, path, to);
    }

    function _burnToken(address[] memory path, uint256 amountIn) private {
        address pair = PancakeLibrary.pairFor(factory, path[0], path[1]);
        uint256 totalSupply = IPancakePair(pair).totalSupply();
        uint256 inputBalance = IERC20(path[0]).balanceOf(pair);
        uint256 liquidity = (amountIn * totalSupply) / inputBalance;

        address funAddress = funAddresses[pair];
        (uint256 amountA, uint256 amountB) = _removeLiquidity(
            path[0],
            path[1],
            liquidity,
            0,
            0,
            funAddress,
            block.timestamp + 300
        
        );
        delete _addressPath;
        _addressPath.push(path[1]);
        _addressPath.push(path[0]);
        _swapExactTokensForTokens(
            amountB,
            0,
            _addressPath,
            death,
            block.timestamp + 300,
            funAddress
        );
        TransferHelper.safeTransferFrom(path[0], funAddress, death, amountA);
    }

    function swapExactTokensForTokens(
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    )
        external
        virtual
        override
        ensure(deadline)
        returns (uint256[] memory amounts)
    {
        if (tokenFee[path[0]] > 0) {
            uint256 amount = (amountIn * tokenFee[path[0]]) / 10000;
            TransferHelper.safeTransferFrom(
                path[0],
                msg.sender,
                rewardPool[path[0]],
                amount
            );
            amountIn = amountIn - amount;
        }

        amounts = PancakeLibrary.getAmountsOut(factory, amountIn, path);
        require(
            amounts[amounts.length - 1] >= amountOutMin,
            "PancakeRouter: INSUFFICIENT_OUTPUT_AMOUNT"
        );

        TransferHelper.safeTransferFrom(
            path[0],
            msg.sender,
            PancakeLibrary.pairFor(factory, path[0], path[1]),
            amounts[0]
        );
        _swap(amounts, path, to);

        address _token = path[path.length - 1];
        if (tokenFee[_token] > 0) {
            uint256 reward = (amounts[path.length - 1] * tokenFee[_token]) /
                10000;
            TransferHelper.safeTransferFrom(
                _token,
                to,
                rewardPool[_token],
                reward
            );
        }

        if (tokenNeedBurn[path[0]]) {
            _burnToken(path, amounts[0]);
        }
    }

