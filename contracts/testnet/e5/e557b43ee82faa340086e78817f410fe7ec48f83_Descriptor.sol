pragma solidity 0.8.13;

import {Ownable2Step} from "openzeppelin/access/Ownable2Step.sol";
import {Strings} from "openzeppelin/utils/Strings.sol";

import {IDescriptor} from "src/interfaces/IDescriptor.sol";

/**
 * @title Descriptor
 */
contract Descriptor is Ownable2Step, IDescriptor {
    using Strings for uint256;

    //* Public variables */

    /**
     * @notice Address of PeonsV2 contract
     */
    address public peonsV2;

    /**
     * @notice Token URI
     */
    string public baseURI;

    /**
     * @notice Constructor
     * @param _peonsV2 Address of PeonsV2 contract
     */
    constructor(address _peonsV2) {
        peonsV2 = _peonsV2;
    }

    //* External view functions */

    /**
     * @notice Returns token URI for a given token ID
     * @param _id Token ID
     * @return Token URI
     */
    function tokenURI(uint256 _id) public view returns (string memory) {
        return string(abi.encodePacked(baseURI, _id.toString()));
    }

    /**
     * @notice Set the base URI for all tokens
     * @dev Only callable by the owner.
     * @param _baseURI Base URI for all tokens
     */
    function setBaseURI(string memory _baseURI) external onlyOwner {
        baseURI = _baseURI;
        emit BaseURISet(baseURI);
    }
}

