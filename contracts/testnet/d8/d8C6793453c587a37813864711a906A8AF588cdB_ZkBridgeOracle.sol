// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import '@openzeppelin/contracts/utils/structs/EnumerableMap.sol';
import "../interface/ILayerZeroOracleV2.sol";
import "../interface/IBlockUpdater.sol";
import "../interface/ILayerZeroEndpoint.sol";
import "../interface/ILayerZeroUltraLightNodeV2.sol";


contract ZkBridgeOracle is ILayerZeroOracleV2, Initializable, OwnableUpgradeable {
    event OracleNotified(uint16 dstChainId, uint16 outboundProofType, uint blockConfirmations, address userApplication, uint fee);
    event WithdrawFee(address receiver, uint256 amount);
    event SetFee(uint16 dstChainId, uint16 outboundProofType, uint256 fee);
    event RemoveFee(uint16 dstChainId, uint16 outboundProofType);
    event ModBlockUpdater(uint16 sourceChainId, address oldBlockUpdater, address newBlockUpdater);
    event EnableSupportedDstChain(uint16 dstChainId);
    event DisableSupportedDstChain(uint16 dstChainId);

    uint16 public proofType;

    mapping(uint16 => mapping(uint16 => uint)) public chainPriceLookup;

    mapping(uint16 => IBlockUpdater) public blockUpdaters;

    mapping(uint16 => bool) public supportedDstChain;

    ILayerZeroEndpoint public layerZeroEndpoint;

    uint256 public totalFee;

    // uln address=> fee amount
    EnumerableMap.AddressToUintMap private feeMap;

    function initialize(address _layerZeroEndpoint) public initializer {
        layerZeroEndpoint = ILayerZeroEndpoint(_layerZeroEndpoint);
        proofType = 1;
        __Ownable_init();
    }

    function updateHash(uint16 _sourceChainId, bytes32 _blockHash, bytes32 _receiptsHash, address _userApplication) external {
        _updateHash(_sourceChainId, _blockHash, _receiptsHash, _userApplication);
    }

    function batchUpdateHash(uint16[] calldata _sourceChainIds, bytes32[] calldata _blockHashes, bytes32[] calldata _receiptsHashes, address[] calldata _userApplication) external {
        require(_sourceChainIds.length == _blockHashes.length, "Parameter lengths must be the same");
        require(_sourceChainIds.length == _receiptsHashes.length, "Parameter lengths must be the same");
        require(_sourceChainIds.length == _userApplication.length, "Parameter lengths must be the same");
        for (uint256 i = 0; i < _sourceChainIds.length; i++) {
            _updateHash(_sourceChainIds[i], _blockHashes[i], _receiptsHashes[i], _userApplication[i]);
        }
    }

    function assignJob(uint16 _dstChainId, uint16 _outboundProofType, uint64 _outboundBlockConfirmation, address _userApplication) external override returns (uint price){
        require(supportedDstChain[_dstChainId], "ZkBridgeOracle:Unsupported dest chain");
        require(_outboundProofType == proofType, "ZkBridgeOracle:Unsupported proof type");
        address uaUln = layerZeroEndpoint.getSendLibraryAddress(_userApplication);
        require(msg.sender == uaUln, "ZkBridgeOracle:invalid sender");
        uint fee = chainPriceLookup[_outboundProofType][_dstChainId];
        _addFee(uaUln, fee);
        emit OracleNotified(_dstChainId, _outboundProofType, _outboundBlockConfirmation, _userApplication, fee);
        return fee;
    }

    function getFee(uint16 _dstChainId, uint16 _outboundProofType, uint64 _outboundBlockConfirmation, address _userApplication) external override view returns (uint price){
        return chainPriceLookup[_outboundProofType][_dstChainId];
    }

    function hashLookup(uint16 _srcChainId, bytes32 _blockHash, bytes32 _receiptsHash, address _userApplication) external view returns (uint256 confirmation){
        address uaUln = layerZeroEndpoint.getReceiveLibraryAddress(_userApplication);
        return ILayerZeroUltraLightNodeV2(uaUln).hashLookup(address(this), _srcChainId, _blockHash, _receiptsHash);
    }

    function feeBalance() external view returns (uint256 balance){
        for (uint256 i = 0; i < EnumerableMap.length(feeMap); i++) {
            (,uint256 feeBalance) = EnumerableMap.at(feeMap, i);
            balance += feeBalance;
        }
    }

    function _updateHash(uint16 _sourceChainId, bytes32 _blockHash, bytes32 _receiptsHash, address _userApplication) internal {
        IBlockUpdater blockUpdater = blockUpdaters[_sourceChainId];
        require(address(blockUpdater) != address(0), "ZkBridgeOracle:Unsupported source chain");
        (bool exist,uint256 blockConfirmation) = blockUpdater.checkBlock(_blockHash, _receiptsHash);
        require(exist, "ZkBridgeOracle:Block Header is not set");
        address uaUln = layerZeroEndpoint.getReceiveLibraryAddress(_userApplication);
        ILayerZeroUltraLightNodeV2(uaUln).updateHash(_sourceChainId, _blockHash, blockConfirmation, _receiptsHash);
    }

    function _addFee(address _uln, uint256 _fee) internal {
        if (_fee > 0) {
            totalFee += _fee;
            (,uint256 balance) = EnumerableMap.tryGet(feeMap, _uln);
            EnumerableMap.set(feeMap, _uln, balance + _fee);
        }
    }

    function _withdrawFee(address _uln, uint256 _fee) internal {
        if (_fee > 0) {
            (,uint256 balance) = EnumerableMap.tryGet(feeMap, _uln);
            EnumerableMap.set(feeMap, _uln, balance - _fee);
        }
    }

    //----------------------------------------------------------------------------------
    // onlyOwner
    function enableSupportedDstChain(uint16 _dstChainId) external onlyOwner {
        supportedDstChain[_dstChainId] = true;
        emit EnableSupportedDstChain(_dstChainId);
    }

    function disableSupportedDstChain(uint16 _dstChainId) external onlyOwner {
        supportedDstChain[_dstChainId] = false;
        emit DisableSupportedDstChain(_dstChainId);
    }

    function setFee(uint16 _dstChainId, uint16 _outboundProofType, uint _price) external onlyOwner {
        require(_price > 0, "ZkBridgeOracle:Price must be greater than zero.");
        chainPriceLookup[_outboundProofType][_dstChainId] = _price;
        emit SetFee(_outboundProofType, _dstChainId, _price);
    }

    function removeFee(uint16 _dstChainId, uint16 _outboundProofType) external onlyOwner {
        require(chainPriceLookup[_outboundProofType][_dstChainId] > 0, "ZkBridgeOracle:The price is already 0.");
        chainPriceLookup[_outboundProofType][_dstChainId] = 0;
        emit RemoveFee(_dstChainId, _outboundProofType);
    }

    function withdrawFee(address payable _to, uint _amount) external override onlyOwner {
        uint256 total = 0;
        for (uint256 i = 0; i < EnumerableMap.length(feeMap); i++) {
            (address uaUln,uint256 feeBalance) = EnumerableMap.at(feeMap, i);
            if (feeBalance > 0) {
                if (total+feeBalance>_amount){
                    feeBalance =_amount-total;
                }
                ILayerZeroUltraLightNodeV2(uaUln).withdrawNative(_to, feeBalance);
                _withdrawFee(uaUln, feeBalance);
                total += feeBalance;
                if (total==_amount){
                    break;
                }
            }
        }
        emit WithdrawFee(_to, total);
    }

    function withdrawFee(address payable _to) external onlyOwner {
        uint256 total = 0;
        for (uint256 i = 0; i < EnumerableMap.length(feeMap); i++) {
            (address uaUln,uint256 feeBalance) = EnumerableMap.at(feeMap, i);
            if (feeBalance > 0) {
                ILayerZeroUltraLightNodeV2(uaUln).withdrawNative(_to, feeBalance);
                _withdrawFee(uaUln, feeBalance);
                total += feeBalance;
            }
        }
        emit WithdrawFee(_to, total);
    }

    function setBlockUpdater(uint16 _sourceChainId, address _blockUpdater) external onlyOwner {
        require(_blockUpdater != address(0), "ZkBridgeOracle:Zero address");
        emit ModBlockUpdater(_sourceChainId, address(blockUpdaters[_sourceChainId]), _blockUpdater);
        blockUpdaters[_sourceChainId] = IBlockUpdater(_blockUpdater);
    }
}

