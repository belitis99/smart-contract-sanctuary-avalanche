pragma solidity ^0.8.13;

import "@openzeppelin/contracts/access/Ownable.sol";

contract DataFormat is Ownable{

    mapping(uint=>Entry) public DemoList;

    struct Entry{
        uint lower;
        uint upper;
        uint value;
    }

    constructor(){
        for(uint i = 0; i < 50; i++){
            if (i==0){
                DemoList[0] = Entry(0, 100, 1);
            }
            else{
                DemoList[i] = Entry(i*100+1, (i+1)*100, i+1);
            }
        }
    }
    function uint2str(uint _i) internal pure returns (string memory _uintAsString) {
        if (_i == 0) {
            return "0";
        }
        uint j = _i;
        uint len;
        while (j != 0) {
            len++;
            j /= 10;
        }
        bytes memory bstr = new bytes(len);
        uint k = len;
        while (_i != 0) {
            k = k-1;
            uint8 temp = (48 + uint8(_i - _i / 10 * 10));
            bytes1 b1 = bytes1(temp);
            bstr[k] = b1;
            _i /= 10;
        }
        return string(bstr);
    }

    function getDemolist() public view returns(string[] memory){
        string[] memory test = new string[](50);
        for(uint i = 0; i < 50; i++){
            test[i] = string.concat("[lower: ", uint2str(DemoList[i].lower));
            string memory upper =  string.concat(", upper: ", uint2str(DemoList[i].upper));
            string memory value = string.concat(", value: ", uint2str(DemoList[i].value));
            test[i] = string.concat(test[i], upper);
            test[i] = string.concat(test[i],value);
            test[i] =  string.concat(test[i], "]");
        }
        return test;
    }
}

