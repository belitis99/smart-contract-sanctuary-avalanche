}

contract SwapV2 {
    using SafeMath for uint;

    //dex variables
    address public routerAdd = 0xd7f655E3376cE2D7A2b08fF01Eb3B1023191A901;
    IDEXRouter public router = IDEXRouter(routerAdd);
 
   function swap (address _for, address from, uint amount) external returns(uint){
       IERC20(from).transferFrom(msg.sender, address(this), amount);
       IERC20(from).approve(routerAdd, amount);
       uint initialBal = IERC20(_for).balanceOf(msg.sender);

        address[] memory path = new address[](2);
        path[0] = from;
        path[1] = _for;      

       router.swapExactTokensForTokens(
            amount,
            0,
            path,
            msg.sender,
            block.timestamp
       );
       uint amountSwap = (IERC20(_for).balanceOf(msg.sender)).sub(initialBal);
       return amountSwap;
   }

    
}