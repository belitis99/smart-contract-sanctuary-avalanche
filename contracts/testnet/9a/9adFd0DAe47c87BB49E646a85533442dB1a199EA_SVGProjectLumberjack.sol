function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
    require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");


    // Pass the multiplier to generateBase64Image
    string memory base64Image = generateBase64Image(tokenId);

    // Concatenate the URI parts.
    string memory json = string(abi.encodePacked('{"name": "', name(), " ", tokenId.toString(), '", "description": "Description of your NFT", "image": "data:image/svg+xml;base64,', base64Image, '"}'));

    // Encode the JSON in base64 format.
    string memory encodedJson = Base64.encode(bytes(json));

    // Return the final token URI.
    return string(abi.encodePacked("data:application/json;base64,", encodedJson));
}



function _mintToken(address to, uint256 multiplier) internal {
    uint256 tokenId = uint256(keccak256(abi.encodePacked(block.timestamp, msg.sender)));
    _safeMint(to, tokenId);

    uint256 start = block.timestamp;
    uint256 end = start + VESTING_PERIOD;
    uint256 totalAmount = tokensPerHour * multiplier * VESTING_PERIOD / (1 hours) ;

    _tokenImages[tokenId] = generateBase64Image(tokenId);

    _vestingInfo[tokenId] = VestingInfo({
        start: start,
        end: end,
        totalAmount: totalAmount,
        claimedAmount: 0
    });

    _tokenMultipliers[tokenId] = multiplier;

    _tokenIdCounter += 1;
}


function mint() external payable {
    uint256 multiplier = 1;

    require(_tokenIdCounter + multiplier <= MAX_SUPPLY, "Maximum NFT supply reached");
    require(msg.value == MINT_PRICE * multiplier, "Incorrect payment amount");

    _mintToken(msg.sender, 1); // Pass 1 as the multiplier for 1x NFT
    
    _tokenIdCounter += (multiplier);

    fundsReceiver.transfer(msg.value);
}

function mint5x() external payable {
    uint256 multiplier = 5;

    require(_tokenIdCounter + multiplier <= MAX_SUPPLY, "Maximum NFT supply reached");
    require(msg.value == MINT_5X_PRICE, "Incorrect payment amount");

        _mintToken(msg.sender, 5); // Pass 5 as the multiplier for 5x NFT
    

    // Update the total token count
    _tokenIdCounter += (multiplier);

    fundsReceiver.transfer(msg.value);
}






    function getVestingInfoByTokenId(uint256 tokenId) public view returns (VestingInfo memory) {
        return _vestingInfo[tokenId];
    }

        function claim(uint256 tokenId) external {
require(_isApprovedOrOwner(msg.sender, tokenId), "Not approved or owner");
require(_exists(tokenId), "Token ID does not exist");

    uint256 claimableAmount = claimablePayout(tokenId);
    require(claimableAmount > 0, "Nothing to claim");

    _vestingInfo[tokenId].claimedAmount += claimableAmount;
    payoutToken.safeTransfer(msg.sender, claimableAmount);

    emit PayoutClaimed(tokenId, msg.sender, claimableAmount);
}

function claimedPayout(uint256 tokenId) public view returns (uint256) {
    return _vestingInfo[tokenId].claimedAmount;
}

function claimAll() external {
    uint256 tokenCount = balanceOf(msg.sender);
    uint256 totalClaimableAmount = 0;

    for (uint256 i = 0; i < tokenCount; i++) {
        uint256 tokenId = tokenOfOwnerByIndex(msg.sender, i);
        uint256 claimableAmount = claimablePayout(tokenId);

        if (claimableAmount > 0) {
            totalClaimableAmount += claimableAmount;
            _vestingInfo[tokenId].claimedAmount += claimableAmount;

            emit PayoutClaimed(tokenId, msg.sender, claimableAmount);
        }
    }

    if (totalClaimableAmount > 0) {
        payoutToken.safeTransfer(msg.sender, totalClaimableAmount);
    }
}

function claimablePayout(uint256 tokenId) public view returns (uint256) {
    VestingInfo memory vesting = _vestingInfo[tokenId];
    uint256 multiplier = _tokenMultipliers[tokenId];
    if (block.timestamp < vesting.start) {
        return 0;
    } else if (block.timestamp >= vesting.end) {
        return vesting.totalAmount - vesting.claimedAmount;
    } else {
        uint256 minutesPassed = (block.timestamp - vesting.start) / (1 minutes);
        uint256 totalClaimable = (minutesPassed * tokensPerHour * multiplier * 1e18) / 60; // Apply the multiplier
        return totalClaimable - vesting.claimedAmount;
    }
}


function vestedAmount(uint256 tokenId) public view returns (uint256) {
    require(_exists(tokenId), "Token ID does not exist");
    VestingInfo memory vesting = _vestingInfo[tokenId];
    return vesting.totalAmount;
}

function nextPayoutAmount(uint256 tokenId) public view returns (uint256) {
    require(_exists(tokenId), "Token ID does not exist");
    return claimablePayout(tokenId);
}

function vestingInfo(uint256 tokenId)
    external
    view
    returns (
        uint256 start,
        uint256 end,
        uint256 totalAmount,
        uint256 claimedAmount
    )
{
    require(_exists(tokenId), "Token ID does not exist");
    VestingInfo memory vesting = _vestingInfo[tokenId];
    return (vesting.start, vesting.end, vesting.totalAmount, vesting.claimedAmount);
}

