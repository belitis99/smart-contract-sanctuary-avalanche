pragma solidity >=0.8.0 <0.9.0;

import "./ERC721APR.sol";
import "./eip/5185/IERC5185.sol";
import "./eip/4906/IERC4906.sol";
import "./TimeCop.sol";
import "./modules/firstDip/IFD.sol";
import "./eip/1155/IERC1155.sol";
import "./eip/1155/MAX1155.sol";

contract FactoryERC721A is ERC721APR
                         , TimeCop
                         , IERC4906
                         , IERC5185UpdatableMetadata {

  string private theURI;
  address public firstDip;
  uint256 public theWLCount;

  constructor(string memory name_, string memory symbol_, address sft) ERC721APR(name_, symbol_) {
    firstDip = sft;
  }

  function baseURI() public view returns (string memory) {
    return _baseURI();
  }

  function _baseURI() internal view virtual override returns (string memory) {
    return theURI;
  }

  function setBaseURI(
    string memory newURI
  ) external
    onlyDev() {
    theURI = newURI;
    emit MetadataUpdates(newURI);
    emit BatchMetadataUpdate(0, type(uint256).max);
  }

  // set the limit on wl
  function setPresaleAmount(
    uint256 quant
  ) external
    onlyDev() {
    theWLCount = quant;
  }

  // Presale Mint
  function preSale(
    uint quant
  ) external
    onlyPresale(){
    // add cost
    // make this sexy later
    if (theWLCount - quant > 0) {
      for (uint i; i < quant;) {
        --theWLCount;
        unchecked { ++i;}
      }
    }

    // mint 1155
    IFD(firstDip).mint(msg.sender, uint256(uint160(address(this))), quant, "");
  }

  // Claim Presale
  function claimPreSale(
    uint quant
  ) external {
    // burn 1155
    IFD(firstDip).burn(msg.sender, uint256(uint160(address(this))), quant, "");
    // mint 721
    _mint(msg.sender, quant);
  }

  // Public Mint
  function publicSale(
    uint quant
  ) external
    onlySale() {
    // add cost
    // check amount of presales out there
    if (this.totalSupply() + IMAX1155(firstDip).getSupplyOf(uint256(uint160(address(this)))) + quant < this.minterCapacity()) {
      _mint(msg.sender, quant);
    }
  }

  function supportsInterface(
    bytes4 interfaceID
  ) external
    view
    virtual
    override (ERC721APR, IERC165, MaxContractRoles)
    returns (bool) {
    // fix this
    }
}

