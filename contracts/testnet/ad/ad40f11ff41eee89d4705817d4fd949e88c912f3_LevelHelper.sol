            leaderTotal.referralAmount = 0;
            leaderTotal.referrerRevenue = 0;
            leaderTotal.count = 0;
            leaderTotal.refCount = 0;
        }

        for (uint256 i = 1; i <= refLevelHandle; i++) {
            Referral storage referrer = userLevelTokenReferral[_user][i][_tokenBuy];
            referrer.referralAmount = 0;
            referrer.referrerRevenue = 0;
            referrer.count = 0;
            referrer.refCount = 0;
            
            Referral storage referrerTotal = userLevelTokenReferralTotal[_user][i][_tokenBuy];
            referrerTotal.referralAmount = 0;
            referrerTotal.referrerRevenue = 0;
            referrerTotal.count = 0;
            referrerTotal.refCount = 0;
        }

        // reset package
        userPackageTimes[_user] = 0;
        userPackageId[_user] = 0;
        totalPackages[packageId] -= 1;

        emit AdminDeleteUserPackage(_user, _tokenBuy, packageId);
    }

    // ======= FOR  USER =====

    function buyPackage(address _tokenBuy, uint256 _packageId, address _refAddress) external nonReentrant {
        uint256 amount = packageTokenFeeAmount[_tokenBuy][_packageId];
        require(amount > 0, "buyPackage: wrong packageID");

        uint256 duration = packageTimes[_packageId];
        require(duration > 0, "buyPackage: error with duration");

        IERC20(_tokenBuy).safeTransferFrom(address(msg.sender), address(this), amount);
        uint256 expiredDate = block.timestamp + duration;

        // handle burn token
        uint256 handleAmount = amount;
        uint256 burnAmount = 0;
        if (tokenBuyBurnPercent[_tokenBuy] > 0) {
            burnAmount = (amount * tokenBuyBurnPercent[_tokenBuy]) / BASIS_POINTS_DIVISOR;
            handleAmount = amount - burnAmount;
            totalBurn[_tokenBuy] += burnAmount;
            
            // burn token
            IERC20(_tokenBuy).safeTransfer(DEAD_ADDRESS, burnAmount);
        }

        // handle active package or upgrade package
        if (userPackageTimes[msg.sender] > block.timestamp) {
            uint256 packageId = getUserPackageId(msg.sender);
            require(_packageId > packageId, "Upgrade: upgrade error require new packageId > current packageId");
            userPackageTimes[msg.sender] += expiredDate;

            emit UpgradePackage(msg.sender, _packageId, handleAmount, userPackageTimes[msg.sender], burnAmount);
        } else {
            // buy new package
            userPackageTimes[msg.sender] = expiredDate;

            emit BuyPackage(msg.sender, _packageId, handleAmount, expiredDate, burnAmount);
        }

        // save packageId
        userPackageId[msg.sender] = _packageId;
        totalRevenue[_tokenBuy] += handleAmount;
        totalPackages[_packageId] += 1;

        // hanlde referral
        if (activeReferral) {
            // ref already exits
            if (refUsers[msg.sender] != address(0)) {
                _handleReferral(msg.sender, refUsers[msg.sender], handleAmount, _tokenBuy, false);
            }

            // new ref
            if (refUsers[msg.sender] == address(0) && _refAddress != address(msg.sender) && _refAddress != address(0)) {
                refUsers[msg.sender] = _refAddress;
                _handleReferral(msg.sender, _refAddress, handleAmount, _tokenBuy, true);
            }
        } else {
            pendingRevenue[owner()][_tokenBuy] += handleAmount;
        }
    }

    function _handleReferral(
        address _user,
        address upline,
        uint256 _amount,
        address _tokenBuy,
        bool _newRef
    ) internal {
        uint256 totalAmountRef = 0;
        uint256 totalAmountLeaderLv1 = 0;
        uint256 totalAmountLeaderLv2 = 0;
        // uint256 itAmount = (itPercent * _amount) / BASIS_POINTS_DIVISOR; // Stack too deep, try removing local variables.

        for (uint256 i = 1; i <= refLevelHandleMax; i++) {
            uint256 packageId = getUserPackageId(upline);
            
            if (upline == address(0) || packageId == 0) {
                break;
            }
            
            // handle referral in 10 level
            if (packageLevels[packageId] >= i) {
                uint256 amountRef = (levelsRefPercent[i] * _amount) / BASIS_POINTS_DIVISOR;
                
                Referral storage referrer = userLevelTokenReferral[upline][i][_tokenBuy];
                referrer.referralAmount += amountRef;
                referrer.referrerRevenue += _amount;
                referrer.count += 1;
                
                Referral storage referrerTotal = userLevelTokenReferralTotal[upline][i][_tokenBuy];
                referrerTotal.referralAmount += amountRef;
                referrerTotal.referrerRevenue += _amount;
                referrerTotal.count += 1;

                if (_newRef) {
                    referrer.refCount += 1;
                    referrerTotal.refCount += 1;
                }
                
                pendingRevenue[upline][_tokenBuy] += amountRef;
                totalAmountRef += amountRef;

                emit HandleReferral(_user, upline, _amount, amountRef, _tokenBuy, _newRef, i);
            }
            
            // handle rewards for leader level 1, 2
            if (_leadersLevel1.contains(upline) || _leadersLevel2.contains(upline)) {
                uint256 percent = _leadersLevel1.contains(upline) ? leaderLv1Percent : leaderLv2Percent;
                uint256 amountReward = (percent * _amount) / BASIS_POINTS_DIVISOR;

                Referral storage leader = leaderTokenReferral[upline][_tokenBuy];
                leader.referralAmount += amountReward;
                leader.referrerRevenue += _amount;
                leader.count += 1;

                Referral storage leaderTotal = leaderTokenReferralTotal[upline][_tokenBuy];
                leaderTotal.referralAmount += amountReward;
                leaderTotal.referrerRevenue += _amount;
                leaderTotal.count += 1;
                
                if (_newRef) {
                    leader.refCount += 1;
                    leaderTotal.refCount += 1;
                }

                if (_leadersLevel1.contains(upline)) {
                    totalAmountLeaderLv1 += amountReward;
                } else {
                    totalAmountLeaderLv2 += amountReward;
                }

                emit HandleReferralLeader(_user, upline, _amount, amountReward, _tokenBuy, _newRef, i);
            }

            upline = refUsers[upline];
        }

        totalITReward[_tokenBuy] += (itPercent * _amount) / BASIS_POINTS_DIVISOR;
        totalReferralReward[_tokenBuy] += totalAmountRef;
        totalLeaderLv1Reward[_tokenBuy] += totalAmountLeaderLv1;
        totalLeaderLv2Reward[_tokenBuy] += totalAmountLeaderLv2;

        require(
            _amount >
                (totalAmountRef +
                    (itPercent * _amount) /
                    BASIS_POINTS_DIVISOR +
                    totalAmountLeaderLv1 +
                    totalAmountLeaderLv2),
            "BuyPackage: Error!"
        );
        pendingRevenue[owner()][_tokenBuy] += (_amount - totalAmountRef);
    }

