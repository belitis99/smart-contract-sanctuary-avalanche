    function viewLeaderLevel1(uint256 cursor, uint256 size) external view returns (address[] memory leaders) {
        uint256 length = size;

        if (length > _leadersLevel1.length() - cursor) {
            length = _leadersLevel1.length() - cursor;
        }

        leaders = new address[](length);

        for (uint256 i = 0; i < length; i++) {
            leaders[i] = _leadersLevel1.at(cursor + i);
        }

        return leaders;
    }

