    /**
     * @notice Claim pending revenue
     */
    function claimPendingRevenue(address _token) external nonReentrant {
        require(!blockUsers[msg.sender], "Claim: abnormal behavior please try again");
        uint256 revenueToClaim = pendingRevenue[msg.sender][_token];
        require(revenueToClaim != 0, "Claim: Nothing to claim");
        pendingRevenue[msg.sender][_token] = 0;

        IERC20(_token).safeTransfer(address(msg.sender), revenueToClaim);

        // reset info users
        if (msg.sender != owner()) {
            for (uint256 i = 1; i <= refLevelHandle; i++) {
                userLevelTokenReferral[msg.sender][i][_token].referralAmount = 0;
                userLevelTokenReferral[msg.sender][i][_token].referrerRevenue = 0;
                userLevelTokenReferral[msg.sender][i][_token].count = 0;
                userLevelTokenReferral[msg.sender][i][_token].refCount = 0;
            }
        }

        emit RevenueClaim(msg.sender, revenueToClaim);
    }
