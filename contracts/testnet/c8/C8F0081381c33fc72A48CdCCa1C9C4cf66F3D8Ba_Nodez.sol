pragma solidity ^0.8.17;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "./ERC721A/ERC721AEnumerable.sol";
import "./interfaces/INodez.sol";
import "./interfaces/IBoostNFT.sol";

/// @title Nodez contract
/// @author 5thWeb
contract Nodez is
    Ownable,
    ReentrancyGuard,
    ERC721AEnumerable,
    ERC721Holder,
    INodez
{
    using SafeERC20 for IERC20;
    using Counters for Counters.Counter;

    uint32 public constant BASE_POINT = 1000;

    /// @dev Apy will be decreased to 90% of previous period.
    uint32 public constant DECREASE_APY = 900;

    /// @dev Apy will be decreased every 9 months.
    uint256 public constant DECREASE_PERIOD = 30 days * 9;

    /// @dev Shows Nodez type by owner and tokenId.
    mapping(address => mapping(uint256 => OwnedNodezInfo))
        public ownedNodezInfos;

    /// @dev Shows staked NFT token ids per each user.
    mapping(address => mapping(uint256 => uint256[])) private stakedNFTIds;

    /// @dev Shows total amount that user received as rewards.
    mapping(address => uint256) public totalEarnings;

    /// @dev Nodez info by types.
    mapping(uint8 => NodezInfo) public nodezInfos;

    /// @dev 2REAL token address.
    /// @dev It's used as reward token.
    address public etherealTokenAddr;

    /// @dev The address of NFT for boosting apy.
    address public boostNFTAddr;

    bool public allowClaim;

    modifier whenClaimAllowed() {
        require(allowClaim, "claim mode is disabled");
        _;
    }

    constructor(
        address _etherealTokenAddr,
        string memory _name,
        string memory _symbol
    ) ERC721A(_name, _symbol) {
        setEtherealTokenAddress(_etherealTokenAddr);
        ++_currentIndex; // Starts tokenId from 1
    }

    /// @inheritdoc INodez
    function setBoostNFT(address _boostNFT) external override onlyOwner {
        require(_boostNFT != address(0), "zero NFT address");
        require(boostNFTAddr == address(0), "already set");

        boostNFTAddr = _boostNFT;
        emit BoostNFTSet(_boostNFT);
    }

    /// @inheritdoc	INodez
    function setNodezInfos(
        uint8[] memory _types,
        NodezInfo[] memory _infos
    ) external override onlyOwner {
        _setNodezInfos(_types, _infos);
    }

    /// @inheritdoc	INodez
    function enableClaim(bool _isAllow) external override onlyOwner {
        allowClaim = _isAllow;
    }

    /// @inheritdoc INodez
    function setEtherealTokenAddress(
        address _etherealTokenAddr
    ) public override onlyOwner {
        require(
            _etherealTokenAddr != address(0),
            "zero ethereal token address"
        );
        etherealTokenAddr = _etherealTokenAddr;
    }

    /// @inheritdoc INodez
    function createNodez(uint8 _nodezType) external override nonReentrant {
        address sender = msg.sender;
        uint256 requiredAmount = nodezInfos[_nodezType].tokenAmountForBuy;
        require(requiredAmount > 0, "invalid nodez type");
        require(
            IERC20(etherealTokenAddr).balanceOf(sender) >= requiredAmount,
            "not enough balance"
        );

        IERC20(etherealTokenAddr).safeTransferFrom(
            sender,
            address(this),
            requiredAmount
        );
        _mint(_nodezType);

        emit NodezCreated(sender, _nodezType);
    }

    /// @inheritdoc INodez
    function upgradeNodez(uint256 _nodezId) external override nonReentrant {
        address sender = msg.sender;
        require(
            _exists(_nodezId) && ownerOf(_nodezId) == sender,
            "not exists or incorrect owner"
        );

        OwnedNodezInfo storage nodezInfo = ownedNodezInfos[sender][_nodezId];
        uint8 nodezType = nodezInfo.nodezType;
        require(nodezType < 6, "max level");

        uint256 requiredAmount = nodezInfos[nodezType].tokenAmountForUpgrade;
        IERC20(etherealTokenAddr).safeTransferFrom(
            sender,
            address(this),
            requiredAmount
        );

        (nodezInfo.pendingRewards, ) = _getPendingRewards(nodezInfo);
        nodezInfo.nodezType = nodezType + 1;
        nodezInfo.lastPickedTimestamp = block.timestamp;
        nodezInfo.apy = nodezInfos[nodezType + 1].apy;

        emit NodezUpgraded(sender, nodezType + 1);
    }

    /// @inheritdoc INodez
    function recycleNodez(uint256 _nodezId) external override {
        address sender = msg.sender;
        require(
            _exists(_nodezId) && ownerOf(_nodezId) == sender,
            "not exists or incorrect owner"
        );

        _burn(_nodezId);

        OwnedNodezInfo storage ownedNodezInfo = ownedNodezInfos[sender][
            _nodezId
        ];
        uint256 stakedNFTLength = ownedNodezInfo.boostedNFTCount;
        if (stakedNFTLength > 0) {
            for (uint256 i = 0; i < stakedNFTLength; i++) {
                uint256 nftTokenId = stakedNFTIds[sender][_nodezId][i];
                IERC721(boostNFTAddr).transferFrom(
                    address(this),
                    sender,
                    nftTokenId
                );
            }
            delete stakedNFTIds[sender][_nodezId];
        }

        (uint256 pendingRewards, ) = _getPendingRewards(ownedNodezInfo);
        if (pendingRewards > 0) {
            IERC20(etherealTokenAddr).safeTransfer(sender, pendingRewards);
        }

        _formatOwnedNodezInfo(ownedNodezInfo);
    }

    /// @inheritdoc INodez
    function pickRewards(
        uint256 _nodezId
    ) external override nonReentrant whenClaimAllowed {
        address sender = msg.sender;
        require(
            _exists(_nodezId) && ownerOf(_nodezId) == sender,
            "not exists or incorrect nodez id"
        );

        OwnedNodezInfo storage nodezInfo = ownedNodezInfos[sender][_nodezId];
        (uint256 pendingRewards, uint32 currentApy) = _getPendingRewards(
            nodezInfo
        );
        require(pendingRewards > 0, "no pickable rewards");
        require(
            IERC20(etherealTokenAddr).balanceOf(address(this)) >=
                pendingRewards,
            "not enough amount for rewards"
        );

        nodezInfo.lastPickedTimestamp = block.timestamp;
        nodezInfo.pendingRewards = 0;
        nodezInfo.apy = currentApy;
        totalEarnings[sender] += pendingRewards;
        IERC20(etherealTokenAddr).safeTransfer(sender, pendingRewards);

        emit RewardsPicked(sender, _nodezId);
    }

    /// @inheritdoc INodez
    function pickAllRewards() external override nonReentrant whenClaimAllowed {
        address sender = msg.sender;
        uint256 length = balanceOf(sender);
        require(sender != address(0), "zero account");
        require(length > 0, "no owned Nodez");

        uint256 totalPendings = 0;
        for (uint256 i = 0; i < length; i++) {
            uint256 nodezId = tokenOfOwnerByIndex(sender, i);

            OwnedNodezInfo storage nodezInfo = ownedNodezInfos[sender][nodezId];
            (uint256 pendingRewards, uint32 currentApy) = _getPendingRewards(
                nodezInfo
            );
            if (pendingRewards == 0) {
                continue;
            }

            nodezInfo.lastPickedTimestamp = block.timestamp;
            nodezInfo.pendingRewards = 0;
            nodezInfo.apy = currentApy;
            totalEarnings[sender] += pendingRewards;
            totalPendings += pendingRewards;
        }

        require(totalPendings > 0, "no pickable rewards");
        require(
            IERC20(etherealTokenAddr).balanceOf(address(this)) >= totalPendings,
            "not enough amount for rewards"
        );

        IERC20(etherealTokenAddr).safeTransfer(sender, totalPendings);

        emit AllRewardsPicked(sender, totalPendings);
    }

    /// @inheritdoc INodez
    function boostAPY(
        uint256[] memory _nftTokenIds,
        uint256 _nodezId
    ) external override {
        address sender = msg.sender;
        require(boostNFTAddr != address(0), "BoostNFT is not set yet");
        require(
            _exists(_nodezId) && ownerOf(_nodezId) == sender,
            "not exists or incorrect nodez id"
        );

        OwnedNodezInfo storage ownedNodezInfo = ownedNodezInfos[sender][
            _nodezId
        ];
        uint8 nodezType = ownedNodezInfo.nodezType;
        NodezInfo memory nodezInfo = nodezInfos[nodezType];
        uint256 length = _nftTokenIds.length;
        require(
            _nftTokenIds.length != 0 &&
                _nftTokenIds.length + ownedNodezInfo.boostedNFTCount <=
                nodezInfo.maxBoostNFTCount,
            "length is not valid"
        );

        uint32 increaseApy = 0;
        for (uint256 i = 0; i < length; i++) {
            uint256 nftTokenId = _nftTokenIds[i];
            IERC721(boostNFTAddr).transferFrom(
                sender,
                address(this),
                nftTokenId
            );
            increaseApy += IBoostNFT(boostNFTAddr).getBoostAPY(nftTokenId);
            stakedNFTIds[sender][_nodezId].push(nftTokenId);
        }

        ownedNodezInfo.boostedNFTCount += uint16(length);
        _increaseAPY(ownedNodezInfo, increaseApy);

        emit APYBoosted(sender, _nodezId, _nftTokenIds);
    }

    /// @inheritdoc INodez
    function importOldNodez(
        ImportNodezInfo[] memory _nodezInfos
    ) external override onlyOwner {
        uint256 length = _nodezInfos.length;
        require(length > 0, "invalid array");

        for (uint256 i = 0; i < length; i++) {
            ImportNodezInfo memory info = _nodezInfos[i];
            address nodezOwner = info.owner;
            require(nodezOwner != address(0), "invalid nodez owner address");
            uint256 nodezCnt = info.nodezCnt;
            uint256 curTokenId = _currentIndex;
            uint8 nodezType = info.nodezType;
            uint32 apy = nodezInfos[nodezType].apy;
            for (uint256 j = 0; j < nodezCnt; j++) {
                ownedNodezInfos[nodezOwner][curTokenId] = OwnedNodezInfo(
                    0,
                    block.timestamp,
                    apy,
                    0,
                    nodezType
                );
                curTokenId++;
            }
            _safeMint(nodezOwner, nodezCnt);
        }
    }

    /// @inheritdoc INodez
    function getROI(uint256 _nodezId) external view override returns (uint256) {
        require(_exists(_nodezId), "not exists nodez id");

        address sender = msg.sender;
        uint8 nodezType = ownedNodezInfos[sender][_nodezId].nodezType;
        NodezInfo memory nodezInfo = nodezInfos[nodezType];
        uint32 apy = nodezInfo.apy;
        return uint256(365 * BASE_POINT) / apy;
    }

    /// @inheritdoc INodez
    function getPendingRewards(
        uint256 _nodezId
    ) external view override returns (uint256) {
        address user = ownerOf(_nodezId);
        require(_exists(_nodezId) && user != address(0), "invalid nodez id");
        (uint256 pendingRewards, ) = _getPendingRewards(
            ownedNodezInfos[user][_nodezId]
        );
        return pendingRewards;
    }

    /// @inheritdoc INodez
    function getAllNodez(
        address _account
    ) public view override returns (OwnedNodezInfo[] memory) {
        require(_account != address(0), "zero user address");
        uint256 length = balanceOf(_account);
        OwnedNodezInfo[] memory allNodez = new OwnedNodezInfo[](length);

        for (uint256 i = 0; i < length; i++) {
            uint256 nodezId = tokenOfOwnerByIndex(_account, i);
            allNodez[i] = ownedNodezInfos[_account][nodezId];
        }

        return allNodez;
    }

    /// @notice Set all Nodez infos.
    /// @dev There are 7 types of Nodez. mini, standard, heavy, master, executive, partner and board.
    /// @dev These infos can be updated by owner.
    /// @param _types The nodez types to set.
    /// @param _infos All Nodez informations.
    function _setNodezInfos(
        uint8[] memory _types,
        NodezInfo[] memory _infos
    ) internal {
        uint8 length = uint8(_infos.length);
        require(length > 0, "invalid array");
        require(length == _types.length, "dismatch array");

        for (uint8 i = 0; i < length; i++) {
            nodezInfos[_types[i]] = _infos[i];
        }
    }

    /// @notice Mint Nodez NFT to `msg.sender`.
    function _mint(uint8 _nodezType) internal {
        uint256 curTokenId = _currentIndex;
        address sender = msg.sender;

        ownedNodezInfos[sender][curTokenId] = OwnedNodezInfo(
            0,
            block.timestamp,
            nodezInfos[_nodezType].apy,
            0,
            _nodezType
        );

        _safeMint(sender, 1);
    }

    /// @inheritdoc INodez
    function getMiningRatePerDay(
        address _holder,
        uint256 _nodezId
    ) external override view returns (uint256) {
        if (!_exists(_nodezId) || ownerOf(_nodezId) != _holder) {
            return 0;
        }
        (uint256 rewardRate, uint32 curApy) = _currentAPY(_holder, _nodezId);
        return (rewardRate * curApy) / 365 / BASE_POINT;
    }

    /// @inheritdoc INodez
    function getTotalMiningRatePerDay(
        address _holder
    ) external override view returns (uint256) {
        uint256 length = balanceOf(_holder);
        if (length == 0) {
            return 0;
        }

        uint256 totalRewardRate = 0;
        for (uint256 i = 0; i < length; i++) {
            uint256 nodezId = tokenOfOwnerByIndex(_holder, i);
            (uint256 rewardRate, uint32 curApy) = _currentAPY(_holder, nodezId);
            totalRewardRate += ((rewardRate * curApy) / BASE_POINT);
        }

        return totalRewardRate / 365;
    }

    function _currentAPY(
        address _holder,
        uint256 _nodezId
    ) internal view returns (uint256, uint32) {
        OwnedNodezInfo memory ownedNodez = ownedNodezInfos[_holder][_nodezId];
        uint8 nodeType = ownedNodez.nodezType;
        NodezInfo memory nodezInfo = nodezInfos[nodeType];
        uint32 currentApy = nodezInfo.apy;

        uint256 startTimestamp = ownedNodez.lastPickedTimestamp;
        uint256 curTimestamp = block.timestamp;

        while (true) {
            if (currentApy == 0 || startTimestamp >= curTimestamp) break;
            currentApy = (currentApy * DECREASE_APY) / BASE_POINT;
            startTimestamp += DECREASE_PERIOD;
        }

        return (nodezInfo.rewardRate, currentApy);
    }

    /// @notice Get pickable rewards from last picked time.
    /// @param _nodezInfo The information of Nodez for picking.
    /// @return pendingRewards The pickable rewards amount.
    /// @return currentApy Current APY.
    function _getPendingRewards(
        OwnedNodezInfo memory _nodezInfo
    ) internal view returns (uint256 pendingRewards, uint32 currentApy) {
        uint8 nodeType = _nodezInfo.nodezType;
        NodezInfo memory nodezInfo = nodezInfos[nodeType];
        uint256 rewardRate = nodezInfo.rewardRate;
        currentApy = nodezInfo.apy;

        uint256 startTimestamp = _nodezInfo.lastPickedTimestamp;
        uint256 curTimestamp = block.timestamp;

        pendingRewards = _nodezInfo.pendingRewards;
        while (true) {
            if (currentApy == 0 || startTimestamp >= curTimestamp) break;
            uint256 calcTimestamp = 0;
            if (startTimestamp + DECREASE_PERIOD >= curTimestamp) {
                calcTimestamp = curTimestamp - startTimestamp;
            } else {
                calcTimestamp = DECREASE_PERIOD;
            }
            pendingRewards += ((rewardRate * currentApy * calcTimestamp) /
                365 days /
                BASE_POINT);
            currentApy = (currentApy * DECREASE_APY) / BASE_POINT;
            startTimestamp += DECREASE_PERIOD;
        }
    }

    /// @notice Increase apy.
    /// @param _nodezInfo The information of Nodez for picking.
    /// @param _addAPY The apy to increase.
    function _increaseAPY(
        OwnedNodezInfo storage _nodezInfo,
        uint32 _addAPY
    ) internal {
        (uint256 pendingRewards, uint32 currentApy) = _getPendingRewards(
            _nodezInfo
        );
        uint32 newAPY = currentApy + _addAPY;
        _nodezInfo.apy = newAPY;
        _nodezInfo.pendingRewards = pendingRewards;
    }

    /// @notice Format nodez information.
    /// @param _nodezInfo The information of Nodez for picking.
    function _formatOwnedNodezInfo(OwnedNodezInfo storage _nodezInfo) internal {
        _nodezInfo.pendingRewards = 0;
        _nodezInfo.lastPickedTimestamp = 0;
        _nodezInfo.apy = 0;
        _nodezInfo.boostedNFTCount = 0;
        _nodezInfo.nodezType = 0;
    }
}

