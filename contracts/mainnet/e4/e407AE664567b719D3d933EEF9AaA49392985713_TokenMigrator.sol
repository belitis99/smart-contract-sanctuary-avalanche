import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";

contract TokenMigrator is IERC721Receiver {
    IERC721 public oldToken;
    IERC721 public newToken;
    address public newTokenHolder;

    constructor(address _oldToken, address _newToken, address _newTokenHolder) {
        oldToken = IERC721(_oldToken);
        newToken = IERC721(_newToken);
        newTokenHolder = _newTokenHolder;
    }

    function onERC721Received(
        address operator,
        address from,
        uint256 tokenId,
        bytes calldata data
    ) external override returns (bytes4) {}

    function migrateToken(uint256 tokenId) public {
        address tokenOwner = oldToken.ownerOf(tokenId);
        require(
            msg.sender == tokenOwner,
            "TokenMigrator: caller is not the token owner"
        );
        oldToken.safeTransferFrom(tokenOwner, newTokenHolder, tokenId);
        newToken.safeTransferFrom(newTokenHolder, tokenOwner, tokenId);
    }

    function oldTokenTransferFrom(
        address from,
        address to,
        uint256 tokenId
    ) external {
        require(
            msg.sender == address(oldToken),
            "TokenMigrator: caller is not the old token"
        );
        oldToken.safeTransferFrom(from, to, tokenId);
    }

    function newtokenTransferFrom(
        address from,
        address to,
        uint256 tokenId
    ) external {
        require(
            msg.sender == address(newToken),
            "TokenMigrator: caller is not the new token"
        );
        newToken.safeTransferFrom(from, to, tokenId);
    }

    function migrateTokens(uint256[] calldata tokenIds) external {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            migrateToken(tokenIds[i]);
        }
    }
}