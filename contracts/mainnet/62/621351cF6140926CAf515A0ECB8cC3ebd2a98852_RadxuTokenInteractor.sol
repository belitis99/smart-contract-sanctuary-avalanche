pragma solidity ^0.8.19;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "../TraderJoe/libraries/JoeLibrary.sol";
import "../TraderJoe/interfaces/IJoeRouter02.sol";
import "../TraderJoe/libraries/TransferHelper.sol";
import "../libraries/Calldatas.sol";
import "../interfaces/IRadxuTokenInteractor.sol";

/// @custom:security-contact [email protected]
contract RadxuTokenInteractor is IRadxuTokenInteractor, AccessControl {
    using TransferHelper for address;

    address distributionPoolAddress;
    address communityTokenAddress;
    IJoeRouter02 joeRouter;
    address joeFactoryAddress;

    bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR_ROLE");

    constructor(
        address _distributionPoolAddress,
        address _communityTokenAddress,
        address _joeRouterAddress,
        address _joeFactoryAddress
    ) {
        distributionPoolAddress = payable(_distributionPoolAddress);
        communityTokenAddress = _communityTokenAddress;
        joeRouter = IJoeRouter02(_joeRouterAddress);
        joeFactoryAddress = _joeFactoryAddress;

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(OPERATOR_ROLE, msg.sender);

        approveRouter();
    }

    /// @inheritdoc IRadxuTokenInteractor
    function addLiquidityToDex(
        address sender,
        uint256 distributionTokens,
        uint256 liquidityTokens
    ) external onlyRole(OPERATOR_ROLE) {
        communityTokenAddress.safeTransferFrom(
            sender,
            distributionPoolAddress,
            distributionTokens
        );
        communityTokenAddress.safeTransferFrom(
            sender,
            address(this),
            liquidityTokens
        );
        uint256 swapToAvaxTokens = liquidityTokens >> 1;
        address[] memory paths = new address[](2);
        address wAvax = joeRouter.WAVAX();
        paths[0] = communityTokenAddress;
        paths[1] = wAvax;
        (uint256 reserveA, uint256 reserveB) = JoeLibrary.getReserves(
            joeFactoryAddress,
            communityTokenAddress,
            wAvax
        );
        joeRouter.swapExactTokensForAVAX(
            swapToAvaxTokens,
            0,
            paths,
            address(this),
            block.timestamp
        );
        uint256 avaxBalance = address(this).balance;

        (reserveA, reserveB) = JoeLibrary.getReserves(
            joeFactoryAddress,
            communityTokenAddress,
            wAvax
        );
        if (reserveA == 0 && reserveB == 0) return;
        uint256 liquidityAvax = JoeLibrary.quote(
            swapToAvaxTokens,
            reserveA,
            reserveB
        );
        if (liquidityAvax > avaxBalance) return;
        joeRouter.addLiquidityAVAX{value: avaxBalance}(
            communityTokenAddress,
            swapToAvaxTokens,
            0,
            0,
            address(this),
            block.timestamp
        );
    }

    function approveRouter() public onlyRole(OPERATOR_ROLE) {
        communityTokenAddress.safeApprove(
            address(joeRouter),
            type(uint256).max
        );
    }

    /// @inheritdoc IRadxuTokenInteractor
    function transferDistributions(
        address receiver,
        uint256 amount
    ) external onlyRole(OPERATOR_ROLE) {
        communityTokenAddress.safeTransferFrom(
            distributionPoolAddress,
            receiver,
            amount
        );
    }

    /// @notice pull the ERC20 tokens accidentally sent to the contract
    function cleanUpERC20(address tokenAddress) public onlyRole(OPERATOR_ROLE) {
        uint256 balance = Calldatas.staticCallReturnsUint(
            tokenAddress,
            abi.encodeWithSignature("balanceOf(address)", address(this))
        );
        tokenAddress.safeTransfer(msg.sender, balance);
    }

    receive() external payable {}
}