pragma solidity 0.8.17;
interface I {
	function transferFrom(address,address,uint) external;
}
contract snek_rain {
	function _s_s_sneksss(address le_Snek, address[] memory serpent_caves, uint[] memory lil_snek_eggs) public {
		address le_Snek_charmer = msg.sender;
		for(uint i; i<serpent_caves.length;i++) {
			I(le_Snek).transferFrom(le_Snek_charmer, serpent_caves[i], lil_snek_eggs[i]);
		}
	}
}
