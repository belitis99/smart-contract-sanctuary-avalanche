interface IBMITokenERC20 is ITokenERC20, IBurnableERC20 {

}

interface IBMITokenERC721 is ITokenERC721, IBurnableERC721 {}

interface IBMITokenERC1155 is ITokenERC1155, IBurnableERC1155 {}

contract UpgradableStakePumpskin is
    Ownable,
    PermissionsEnumerable,
    ReentrancyGuard,
    Upgradeable,
    Initializable
{
    // using SafeERC20 for ERC20Base;
    using SafeMath for uint256;

    // Interfaces for ERC20 and ERC721
    IBMITokenERC20 public pieToken;
    IBMITokenERC20 public potionToken;
    IBMITokenERC721 public pumpskinCollection;
    IBMITokenERC1155 public powerupCollection;

    // redundant struct - can't be packed? (max totalML = 167,772,160,000)
    uint40 public totalML;
    uint16 public totalStakedPumpkin;

    // the base number of $PPIE per pumpskin (i.e. 0.75 $ppie)
    uint256 public BASE_HOLDER_PIES = 30000000000000000;

    // the number of $PPIE per pumpskin per day per ml (i.e. 0.25 $ppie /pumpskin /day /ml)
    uint256 public PIES_PER_DAY_PER_ML = 10000000000000000;

    // how much pie it costs to skip the cooldown
    uint256 public COOLDOWN_BASE = 100000000000000000000; // base 100
    // how much additional pie it costs to skip the cooldown per ml
    uint256 public COOLDOWN_BASE_FACTOR = 100000000000000000000; // additional 100 per ml
    // how long to wait before skip cooldown can be re-invoked
    uint256 public COOLDOWN_CD_IN_SECS = 86400; // additional 100 per ml

    uint256 public LEVELING_BASE = 25;
    uint256 public LEVELING_RATE = 2;
    uint256 public COOLDOWN_RATE = 3600; // 60 mins

    uint256 MAX_POWERUP_LIMIT = 3;

    // uint8 (0 - 255)
    // uint16 (0 - 65535)
    // uint24 (0 - 16,777,216)
    // uint32 (0 - 4,294,967,295)
    // uint40 (0 - 1,099,511,627,776)
    // unit48 (0 - 281,474,976,710,656)
    // uint256 (0 - 1.157920892e77)

    /**
     * Stores staked pumpskin fields (=> 152 <= stored in order of size for optimal packing!)
     */
    struct StakedPumpskinObj {
        // is pumpskin Staked
        bool isStaked;
        // TokenId of pumpskin being staked
        uint256 tokenId;
        // the current ml level (0 -> 16,777,216)
        uint256 ml;
        // when to calculate pie from (max 20/02/36812, 11:36:16)
        uint32 sinceTs;
        // for the skipCooldown's cooldown (max 20/02/36812, 11:36:16)
        uint32 lastSkippedTs;
        // how much this pumpskin has been fed (in whole numbers)
        uint48 eatenAmount;
        // cooldown time until level up is allow (per ml)
        uint32 cooldownTs;
    }

    // define Powerup struct
    struct PowerUp {
        uint256 id;
        uint256 startTs;
        uint256 endTs;
    }

    // mapping of powerup id to powerup rewards pre hour
    mapping(uint256 => uint256) public powerupRewardRate;
    // mapping of powerup id to powerup duration
    mapping(uint256 => uint256) public powerupDuration;
    // mapping of pumpskin token id to StakedPumpskinObject
    mapping(uint256 => StakedPumpskinObj) public allPumpskins;
    // mapping of pumpskin token id to activePowerUp
    mapping(uint256 => PowerUp[]) public activePowerups;

    // Events
    event Minted(address owner, uint256 piesAmount);
    event BurnedPies(address owner, uint256 piesAmount);
    event BurnedPotion(address owner, uint256 piesAmount);
    event Staked(uint256 tokenId, uint256 timeStamp);
    event UnStaked(uint256 tokenId, uint256 timeStamp);
    event LevelUp(uint256 tokenId, uint256 newML);
    event NameChange(uint256 tokenId, string name);

    // initializer function to set the rewards token and the NFT collection addresses
    function initialize() external initializer {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupOwner(msg.sender);
    }

    function _authorizeUpgrade(
        address
    ) internal view override onlyRole(DEFAULT_ADMIN_ROLE) {}

    function _canSetOwner() internal view virtual override returns (bool) {
        return msg.sender == owner();
    }

    function setTokens(
        IBMITokenERC20 _pieToken,
        IBMITokenERC20 _potionToken,
        IBMITokenERC721 _pumpskinCollection,
        IBMITokenERC1155 _powerupCollection
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        pieToken = IBMITokenERC20(_pieToken);
        potionToken = IBMITokenERC20(_potionToken);
        pumpskinCollection = IBMITokenERC721(_pumpskinCollection);
        powerupCollection = IBMITokenERC1155(_powerupCollection);
    }

    // How much is required to be fed to level up per ml
    function potionLevelingRate(uint256 ml) public view returns (uint256) {
        // need to divide the ml by 100, and make sure the potion level is at 18 decimals
        return LEVELING_BASE * ((ml / 100) ** LEVELING_RATE);
    }

    // when using the value, need to add the current block timestamp as well
    function cooldownRate(uint256 ml) public view returns (uint256) {
        // need to divide the ml by 100

        return (ml / 100) * COOLDOWN_RATE;
    }

    // upgrade the contract
    function _setML(uint256 _tokenId, uint256 _newML) internal {
        StakedPumpskinObj memory pumpskin = allPumpskins[_tokenId];
        pumpskin.ml = _newML;
        // set and update that token in the mapping
        allPumpskins[_tokenId] = pumpskin;
        emit LevelUp(_tokenId, _newML);
    }

    function setML(
        uint256 _tokenId,
        uint256 _newML
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _setML(_tokenId, _newML);
    }

    // stake pumpskin, check if is already staked, get all detail for pumpskin such as
    function _stake(uint256 tid) internal {
        // verify user is the owner of the pumpskin...
        require(pumpskinCollection.ownerOf(tid) == msg.sender, "NOT OWNER");

        StakedPumpskinObj memory pumpskin = allPumpskins[tid];

        // verify pumpskin is not already staked
        require(pumpskin.isStaked != true, "ALREADY STAKED");

        if (pumpskin.ml <= 0) {
            pumpskin.ml = 100;
            allPumpskins[tid] = pumpskin;
        }

        // if lastSkippedTs is 0 its mean it never have a last skip timestamp
        uint32 ts = uint32(block.timestamp);

        // create staked pumpskin...
        allPumpskins[tid] = StakedPumpskinObj(
            true,
            tid,
            uint24(pumpskin.ml),
            ts,
            pumpskin.lastSkippedTs > 0
                ? pumpskin.lastSkippedTs
                : uint32(ts - COOLDOWN_CD_IN_SECS),
            uint48(0),
            uint32(ts) + uint32(cooldownRate(pumpskin.ml))
        );

        // update snapshot values...
        // N.B. could be optimised for multi-stakes - but only saves 0.5c AUD per pumpskin - not worth it, this is a one time operation.
        totalStakedPumpkin += 1;
        totalML += uint24(pumpskin.ml);

        emit Staked(tid, block.timestamp);
    }

    function stake(uint256[] calldata tids) external nonReentrant {
        for (uint256 i = 0; i < tids.length; i++) {
            _stake(tids[i]);
        }
    }

    /**
     * Calculates the amount of pie that is claimable from a pumpskin.
     */
    function claimableView(uint256 tokenId) public view returns (uint256) {
        StakedPumpskinObj memory c = allPumpskins[tokenId];
        uint256 powerUpMultiplier = 1;
        if (c.ml > 0) {
            PowerUp[] memory ap = activePowerups[tokenId];
            // loop through all the active powerups in the pumpskin
            for (uint256 i = 0; i < ap.length; i++) {
                uint256 powerupDelta = block.timestamp - ap[i].startTs;
                if (powerupDelta < powerupDuration[ap[i].id]) {
                    powerUpMultiplier += powerupRewardRate[ap[i].id];
                }
            }

            uint256 piePerDay = ((PIES_PER_DAY_PER_ML * (c.ml / 100)) +
                BASE_HOLDER_PIES);

            uint256 deltaSeconds = block.timestamp - c.sinceTs;
            return deltaSeconds * (piePerDay / 86400) * powerUpMultiplier;
        } else {
            return 0;
        }
    }

    // /**
    //  * Calculates the TOTAL amount of pie that is claimable from ALL pumpskins.
    //  */
    function claimableViewMultiple(
        uint256[] calldata tokenIds
    ) public view returns (uint256) {
        uint256 totalClaimable = 0;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            totalClaimable += claimableView(tokenIds[i]);
        }
        return totalClaimable;
    }

    /**
     * Claims pies from the provided pumpskins.
     */
    function _claimPies(uint256[] calldata tokenIds) internal {
        // IERC721 x = pumpskinCollection;
        uint256 totalClaimablePie = 0;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(
                pumpskinCollection.ownerOf(tokenIds[i]) == msg.sender,
                "NOT OWNER"
            );
            StakedPumpskinObj memory pumpskin = allPumpskins[tokenIds[i]];
            // we only care about pumpskin that have been staked (i.e. ml > 0) ...
            if (pumpskin.ml > 0) {
                uint256 claimablePie = claimableView(tokenIds[i]);
                if (claimablePie > 0) {
                    totalClaimablePie = totalClaimablePie + claimablePie;
                    // reset since, for the next calc...
                    pumpskin.sinceTs = uint32(block.timestamp);
                    allPumpskins[tokenIds[i]] = pumpskin;
                    PowerUp[] storage ap = activePowerups[tokenIds[i]];
                    // loop through all the active powerups in the pumpskin
                    // and remove any that have expired
                    for (uint256 j = 0; j < ap.length; j++) {
                        uint256 powerupDelta = block.timestamp - ap[j].startTs;
                        if (powerupDelta >= powerupDuration[ap[j].id]) {
                            // remove the powerup from the array
                            ap[j] = ap[ap.length - 1];
                            ap.pop();
                        }
                    }
                }
            }
        }
        if (totalClaimablePie > 0) {
            // pieToken._mint(msg.sender, totalClaimablePie);
            pieToken.mintTo(msg.sender, totalClaimablePie);
            emit Minted(msg.sender, totalClaimablePie);
        }
    }

    /**
     * Claims pies from the provided pumpskins.
     */
    function claimPies(uint256[] calldata tokenIds) external {
        _claimPies(tokenIds);
    }

    /**
     * Unstakes a pumpskin. Why you'd call this, I have no idea.
     */
    function _unstake(uint256 tokenId) internal {
        require(pumpskinCollection.ownerOf(tokenId) == msg.sender, "NOT OWNER");
        StakedPumpskinObj memory c = allPumpskins[tokenId];

        totalML -= uint24(c.ml);
        totalStakedPumpkin -= 1;
        c.isStaked = false;
        allPumpskins[tokenId] = c;

        // let ppl know!
        emit UnStaked(tokenId, block.timestamp);
    }

    function _unstakeMultiple(uint256[] calldata tids) internal {
        for (uint256 i = 0; i < tids.length; i++) {
            _unstake(tids[i]);
        }
    }

    /**
     * Unstakes MULTIPLE pumpskin. Why you'd call this, I have no idea.
     */
    function unstake(uint256[] calldata tids) external nonReentrant {
        _unstakeMultiple(tids);
    }

    /**
     * Unstakes MULTIPLE pumpskin AND claims the pies.
     */
    function withdrawAllPumpkinAndClaim(uint256[] calldata tids) external {
        _claimPies(tids);
        _unstakeMultiple(tids);
    }

    /**
     * Public : update the pumpskin's Magic level.
     */
    function levelUpPumpkin(uint256 tid) public {
        StakedPumpskinObj memory c = allPumpskins[tid];
        require(c.ml > 0, "NOT STAKED");

        // check: pumpskin has eaten enough...
        require(c.eatenAmount >= potionLevelingRate(c.ml), "MORE FOOD REQD");
        // check: cooldown has passed...
        require(block.timestamp >= c.cooldownTs, "COOLDOWN NOT MET");

        // increase ml, reset eaten to 0, update next potion level and cooldown time
        c.ml = c.ml + 100;
        c.eatenAmount = 0;
        c.cooldownTs = uint32(block.timestamp + cooldownRate(c.ml));
        allPumpskins[tid] = c;

        // need to increase overall size
        totalML += uint24(100);

        // and update the pumpskin contract
        _setML(tid, c.ml);
    }

    function levelUpPumpkinMultiple(uint256[] calldata tids) external {
        for (uint256 i = 0; i < tids.length; i++) {
            levelUpPumpkin(tids[i]);
        }
    }

    /**
     * Internal: burns the given amount of pies from the wallet.
     */
    function _burnPies(address sender, uint256 piesAmount) internal {
        // NOTE do we need to check this before burn?
        require(pieToken.balanceOf(sender) >= piesAmount, "NOT ENOUGH PIE");
        pieToken.burnFrom(sender, piesAmount);
        emit BurnedPies(sender, piesAmount);
    }

    /**
     * Burns the given amount of pies from the sender's wallet.
     */
    function burnPies(
        address sender,
        uint256 piesAmount
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _burnPies(sender, piesAmount);
    }

    /**
     * Skips the "levelUp" cooling down period, in return for burning Pie.
     */
    function skipCoolingOff(uint256 tokenId, uint256 pieAmt) external {
        StakedPumpskinObj memory pumpskin = allPumpskins[tokenId];
        require(pumpskin.ml != 0, "NOT STAKED");

        uint32 ts = uint32(block.timestamp);

        // NOTE Does it matter if sender is not owner?
        // Pumpskin instance = Pumpskin(PUMPSKIN_CONTRACT);
        // require(instance.ownerOf(pumpskinId) == msg.sender, "NOT OWNER");

        // check: enough pie in wallet to pay
        uint256 walletBalance = pieToken.balanceOf(msg.sender);
        require(walletBalance >= pieAmt, "NOT ENOUGH PIE IN WALLET");

        // check: provided pie amount is enough to skip this level
        require(
            pieAmt >= checkSkipCoolingOffAmt(pumpskin.ml),
            "NOT ENOUGH PIE TO SKIP"
        );

        // check: user hasn't skipped cooldown in last 24 hrs
        require(
            (pumpskin.lastSkippedTs + COOLDOWN_CD_IN_SECS) <= ts,
            "BLOCKED BY 24HR COOLDOWN"
        );

        // burn pies

        _burnPies(msg.sender, pieAmt);

        // disable cooldown
        pumpskin.cooldownTs = ts;
        // track last time cooldown was skipped (i.e. now)
        pumpskin.lastSkippedTs = ts;
        allPumpskins[tokenId] = pumpskin;
    }

    /**
     * Calculates the cost of skipping cooldown.
     */
    function checkSkipCoolingOffAmt(uint256 ml) public view returns (uint256) {
        // NOTE cannot assert KG is < 100... we can have large numbers!
        return ((ml / 100) * COOLDOWN_BASE_FACTOR);
    }

    /**
     * potion hydrating the pumpskin
     */
    function hydratePumpskin(uint256 tokenId, uint256 potionAmount) public {
        StakedPumpskinObj memory pumpskin = allPumpskins[tokenId];
        require(pumpskin.isStaked == true, "NOT STAKED");
        require(potionAmount > 0, "NOTHING TO POTION");
        require(pumpskinCollection.ownerOf(tokenId) == msg.sender, "NOT OWNER");
        require(
            potionToken.balanceOf(msg.sender) >= potionAmount,
            "NOT ENOUGH POTION"
        );
        require(
            potionLevelingRate(pumpskin.ml) - pumpskin.eatenAmount <=
                potionAmount,
            "TOO FULL"
        );

        // burn potion...
        potionToken.burnFrom(msg.sender, potionAmount);
        emit BurnedPotion(msg.sender, potionAmount);

        // update the block time as well as claimable
        pumpskin.eatenAmount =
            uint48(potionAmount / 1e18) +
            pumpskin.eatenAmount;
        allPumpskins[tokenId] = pumpskin;
    }

    function hydratePumpskinMultilple(
        uint256[] calldata tids,
        uint256[] calldata amounts
    ) external {
        for (uint256 i = 0; i < tids.length; i++) {
            hydratePumpskin(tids[i], amounts[i]);
        }
    }

