    // Set powerup reward rate
    function setPowerupRewardRate(
        uint256 powerupId,
        uint256 rewardRate
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        powerupRewardRate[powerupId] = rewardRate;
    }

    // Set powerup duration
    function setPowerupDuration(
        uint256 powerupId,
        uint256 duration
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        powerupDuration[powerupId] = duration;
    }

    // set powerup limit
    function setPowerupLimit(
        uint256 limit
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        MAX_POWERUP_LIMIT = limit;
    }

    // Activate powerup
    function activatePowerup(
        uint256 tokenId,
        uint256 powerupId
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        StakedPumpskinObj memory pumpskin = allPumpskins[tokenId];
        PowerUp[] storage ap = activePowerups[tokenId];
        require(pumpskin.ml > 0, "NOT STAKED");
        require(powerupRewardRate[powerupId] > 0, "CAN'T FIND REWARD RATE");
        require(powerupDuration[powerupId] > 0, "CAN'T FIND DURATION");
        require(ap.length < MAX_POWERUP_LIMIT, "MAX POWERUP LIMIT");
        powerupCollection.burn(msg.sender, powerupId, 1);
        uint256 pd = powerupDuration[powerupId];
        ap.push(
            PowerUp(
                powerupId,
                uint256(block.timestamp),
                uint256(block.timestamp + pd)
            )
        );
        allPumpskins[tokenId] = pumpskin;
    }

