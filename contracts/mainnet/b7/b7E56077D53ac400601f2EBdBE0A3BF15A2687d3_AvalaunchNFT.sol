        int128 _start,
        int128 _end,
        uint256 _lastIndex
    ) private view returns (uint256) {
        uint256 positionToAssume = _lastIndex;
        for (uint256 i = 0; i < _lastIndex; i++) {
            int128 rangeStart = _ranges[i].start;
            int128 rangeEnd = _ranges[i].end;
            if (_start < rangeStart && positionToAssume == _lastIndex) {
                positionToAssume = i;
