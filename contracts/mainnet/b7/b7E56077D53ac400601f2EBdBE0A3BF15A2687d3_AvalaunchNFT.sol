        lastTokenRevealed += revealBatchSize;

        emit Reveal(_batchNumber, _seed);
    }

    /**
     * @notice Internal function for retrieving current batch number
     */
    function _getCurrentBatchNumber() internal view returns (uint256 _batchNumber) {
        unchecked {
            _batchNumber = lastTokenRevealed / revealBatchSize;
        }
    }

    /**
     * @notice function to reveal the next batch
     */
    function _reveal(uint256 _totalSupply) internal {
        uint256 _batchNumber = _getAvailableBatch(_totalSupply);

        if (!isUsingVRF) {
            lastTokenRevealed += revealBatchSize;
            _setBatchSeed(_batchNumber);
            emit Reveal(_batchNumber, batchToSeed[_batchNumber]);
