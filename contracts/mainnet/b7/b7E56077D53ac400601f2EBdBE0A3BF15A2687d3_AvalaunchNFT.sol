    /**
     * @notice function to initialize Chainlink VRF configuration
     * @param _vrfCoordinator is address of a Chainlink coordinator
     * @param _keyHash is the keyhash of wanted gas lane
     * @param _subId is project's subscription id
     * @param _callbackGasLimit gas limit for the coordinator callbacks
     */
    function setVRF(
        address _vrfCoordinator,
        bytes32 _keyHash,
        uint64 _subId,
        uint32 _callbackGasLimit
    ) external onlyOwner {
        // Once vrf is set, settings are unchangeable
        if (isUsingVRF) {
            revert AvalaunchNFT__VRFAlreadySet();
        }

        // Set coordinator
        _setVRFCoordinator(_vrfCoordinator);

        // Get configuration from coordinator
        (, uint32 s_ConfigMaxGasLimit, bytes32[] memory s_provingKeyHashes) = VRFCoordinatorV2Interface(_vrfCoordinator)
            .getRequestConfig();

        // Check callbackGasLimit value
        if (_callbackGasLimit < 20_000 || _callbackGasLimit > s_ConfigMaxGasLimit) {
            revert AvalaunchNFT__InvalidCallbackGasLimit();
        }

        // Check if _keyHash exists among coordinators keyHashes
        bool isKeyHashFound;
        for (uint256 i; i < s_provingKeyHashes.length; i++) {
            if (s_provingKeyHashes[i] == _keyHash) {
                isKeyHashFound = true;
                break;
            }
        }

        if (!isKeyHashFound) {
            revert AvalaunchNFT__InvalidKeyHash();
        }

        // Get subscription consumers
        (, , , address[] memory consumers) = VRFCoordinatorV2Interface(_vrfCoordinator).getSubscription(_subId);

        // Check if this address is among consumers
        bool isConsumer;
        for (uint256 i; i < consumers.length; i++) {
            if (address(this) == consumers[i]) {
                isConsumer = true;
                break;
            }
        }

        if (!isConsumer) {
            revert AvalaunchNFT__ContractIsNotConsumer();
        }

        // Set values
        isUsingVRF = true;
        keyHash = _keyHash;
        subId = _subId;
        callbackGasLimit = _callbackGasLimit;

        emit VRFSet(_vrfCoordinator, _keyHash, _subId, _callbackGasLimit);
