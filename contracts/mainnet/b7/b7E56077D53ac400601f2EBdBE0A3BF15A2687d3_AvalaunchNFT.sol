            emit RevealPending(_batchNumber);
        }
    }

    /**
     * @notice function to get available batch if there is one
     * @param _totalSupply current total supply of nfts
     */
    function _getAvailableBatch(uint256 _totalSupply) internal view returns (uint256 _batchNumber) {
        _batchNumber = _getCurrentBatchNumber();

        if (_totalSupply < lastTokenRevealed + revealBatchSize || vrfRequested[_batchNumber]) {
            revert BatchReveal__NoBatchAvailable();
        }
    }

    uint256[49] private __gap;
}

contract AvalaunchNFT is
    IAvalaunchNFT,
    ERC721AUpgradeable,
    ERC2981Upgradeable,
    BatchReveal,
