    }

    /**
     * @notice function to reveal the next batch of nfts
     */
    function reveal() external onlyOwner {
        _reveal(totalSupply());
    }

    /**
     * @notice function to retrieve current available batch if there's any
     */
    function getAvailableBatch() external view returns (uint256) {
        return _getAvailableBatch(totalSupply());
    }

    /**
     * @notice function to set base uri (and suffix)
     * @dev callable by owner only
     * @param baseURI_ is base uri for computing {tokenURI}
     */
    function setBaseURI(string calldata baseURI_, string calldata suffix_) external onlyOwner {
        if (bytes(baseURI).length != 0) {
            revert AvalaunchNFT__BaseURIAlreadySet();
        }
        baseURI = baseURI_;
        suffix = suffix_;
        emit BaseURISet(baseURI_, suffix_);
    }

    /**
     * @notice function to set unrevealed uri
     * @param _unrevealedURI is uri for unrevealed nfts
     */
    function setUnrevealedURI(string calldata _unrevealedURI) external onlyOwner {
        if (bytes(unrevealedURI).length != 0) {
            revert AvalaunchNFT__unrevealedURIAlreadySet();
        }
        unrevealedURI = _unrevealedURI;
        emit UnrevealedURISet(_unrevealedURI);
    }

    /**
     * @notice function to retrieve token uri
     * @dev returns unrevealedURI if token is not revealed yet
     * @param id is tokenId
     */
    function tokenURI(
        uint256 id
    ) public view override(ERC721AUpgradeable, IERC721MetadataUpgradeable) returns (string memory) {
        if (id >= lastTokenRevealed) {
            return unrevealedURI;
        } else {
            return string(abi.encodePacked(baseURI, _getShuffledTokenId(id).toString(), suffix));
        }
    }

    /**
     * @notice function to set default royalty settings
     * @dev only owner
     * @param _receiver is royalty receiver address
     * @param _feeNumerator is percentage of fees represented as a fragment of feeDenominator
     */
    function setDefaultRoyalty(address _receiver, uint96 _feeNumerator) external onlyOwner {
        // Limit royalty to 15%
        if (_feeNumerator > 1_500) {
            revert AvalaunchNFT__InvalidRoyaltySettings();
        }
        _setDefaultRoyalty(_receiver, _feeNumerator);
        emit DefaultRoyaltySet(_receiver, _feeNumerator);
    }

    /**
     * @notice function to withdraw earnings of NFT sale
     * @param to is fund receiver
     */
    function withdrawEarnings(address to) external onlyOwner {
        if (to == address(0)) {
            revert AvalaunchNFT__InvalidAddress();
        }
        uint256 amount = address(this).balance;
        if (amount == 0) {
            revert AvalaunchNFT__BalanceEmpty();
        }
        (bool success, ) = to.call{value: amount}("");
        if (!success) {
            revert AvalaunchNFT__AVAXTransferFailed();
        }
        emit EarningsWithdrawn(to, amount);
    }

    /**
     * @dev Returns true if this contract implements the interface defined by
     * `interfaceId`. See the corresponding
     * https://eips.ethereum.org/EIPS/eip-165#how-interfaces-are-identified[EIP section]
     * to learn more about how these ids are created.
     *
     * This function call must use less than 30 000 gas.
     */
    function supportsInterface(
        bytes4 interfaceId
    ) public view virtual override(ERC721AUpgradeable, ERC2981Upgradeable, IERC165Upgradeable) returns (bool) {
        return
            interfaceId == type(IAvalaunchNFT).interfaceId ||
            ERC165Upgradeable.supportsInterface(interfaceId) ||
            ERC721AUpgradeable.supportsInterface(interfaceId) ||
            ERC2981Upgradeable.supportsInterface(interfaceId) ||
            super.supportsInterface(interfaceId);
    }
}