    function initialize(
        address owner,
        address royaltyReceiver,
        uint256 collectionSize,
        uint256 revealBatchSize,
        string calldata collectionName,
        string calldata collectionSymbol
    ) external initializer {
        // Initialize inheritances
        __ERC721A_init(collectionName, collectionSymbol);
        __ERC2981_init();
        __ReentrancyGuard_init();

        // Initialize batch reveal
        __BatchReveal_init(collectionSize, revealBatchSize);

        // Default royalty value is 3% considering denominator value is 10k
        _setDefaultRoyalty(royaltyReceiver, 300);

        if (owner == address(0)) {
            revert AvalaunchNFT__InvalidAddress();
        }

        // Transfer ownership to project owner
        _transferOwnership(owner);
