    event Reveal(uint256 batchNumber, uint256 batchSeed);
    event RevealPending(uint256 batchNumber);

    struct Range {
        int128 start;
        int128 end;
    }

    /**
     * @notice Internal initialization function
     * @param _collectionSize is amount of tokens that can be minted in this collection
     * @param _revealBatchSize amount of tokens which will be revealed in single iteration
     */
    function __BatchReveal_init(uint256 _collectionSize, uint256 _revealBatchSize) internal onlyInitializing {
        // Make parameter sanity check
        if (_revealBatchSize == 0 || _collectionSize % _revealBatchSize != 0 || _revealBatchSize > _collectionSize) {
            revert BatchReveal__InvalidInitParams();
        }
