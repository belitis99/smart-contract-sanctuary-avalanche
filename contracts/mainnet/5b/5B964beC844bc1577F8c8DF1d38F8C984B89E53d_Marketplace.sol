pragma solidity 0.8.11;

import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/utils/cryptography/SignatureChecker.sol';
import 'erc721a/contracts/IERC721A.sol';
import './interfaces/IMarketplace.sol';
import './interfaces/IRegistry.sol';
import './extensions/EIP712.sol';

contract Marketplace is IMarketplace, EIP712, Ownable {
    string public constant name = 'NFTTIX Marketplace';
    string public constant version = '1.0';
    address public immutable registry;

    mapping(bytes32 => bool) canceled;
    mapping(bytes32 => bool) fills;

    event OrderFilled(OrderInterface.Order order, address filledBy);
    event OrderCanceled(OrderInterface.Order order);

    constructor(address _registry, uint256 chainId) {
        DOMAIN_SEPARATOR = hash(EIP712Domain({ name: name, version: version, chainId: chainId, verifyingContract: address(this) }));
        registry = _registry;
        transferOwnership(registry);
    }

    function hashToSign(bytes32 orderHash) internal view returns (bytes32 hash) {
        return keccak256(abi.encodePacked('\x19\x01', DOMAIN_SEPARATOR, orderHash));
    }

    function fillOrder(
        OrderInterface.Order memory order,
        bytes memory signature,
        address buyer
    ) external onlyOwner {
        _fillOrder(order, signature, buyer);
    }

    function cancelOrder(OrderInterface.Order memory order, bytes memory signature) external onlyOwner {
        _cancelOrder(order, signature);
    }

    function _fillOrder(
        OrderInterface.Order memory order,
        bytes memory signature,
        address buyer
    ) internal {
        bytes32 orderHash = OrderInterface.hashOrder(order);
        require(!fills[orderHash], 'Order is already filled.');
        require(!canceled[orderHash], 'Order is canceled.');
        bytes32 calculatedHashToSign = hashToSign(orderHash);

        // Provide a single mechanism to verify both private-key (EOA) ECDSA signature and ERC1271 contract sigantures.
        require(SignatureChecker.isValidSignatureNow(order.maker, calculatedHashToSign, signature), 'Invalid signature or incorrect hash');

        fills[orderHash] = true;
        emit OrderFilled(order, buyer);
    }

    function _cancelOrder(OrderInterface.Order memory order, bytes memory signature) internal {
        bytes32 orderHash = OrderInterface.hashOrder(order);
        require(!canceled[orderHash], 'Order is already canceled.');
        bytes32 calculatedHashToSign = hashToSign(orderHash);

        // Provide a single mechanism to verify both private-key (EOA) ECDSA signature and ERC1271 contract sigantures.
        require(SignatureChecker.isValidSignatureNow(order.maker, calculatedHashToSign, signature), 'Invalid signature or incorrect hash');

        canceled[orderHash] = true;
        emit OrderCanceled(order);
    }
}

