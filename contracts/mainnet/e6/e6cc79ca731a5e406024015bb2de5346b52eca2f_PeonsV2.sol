pragma solidity 0.8.13;

import {OZNFTBaseUpgradeable, IOZNFTBaseUpgradeable} from "nft-base-contracts/OZNFTBaseUpgradeable.sol";

import {IPeonsV2} from "src/interfaces/IPeonsV2.sol";
import {IDescriptor} from "src/interfaces/IDescriptor.sol";

/**
 * @title Peons V2 ERC-721 token
 */
contract PeonsV2 is OZNFTBaseUpgradeable, IPeonsV2 {
    /**
     * @notice PeonMinter contract address
     */
    address public peonMinter;

    /**
     * @notice Descriptor contract
     */
    IDescriptor public descriptor;

    /**
     * @notice collectionSize
     */
    uint256 public collectionSize;

    /**
     * @notice Checks if function is called from PeonMinter contract
     */
    modifier onlyPeonMinter() {
        if (msg.sender != peonMinter) revert PeonsV2__OnlyPeonMinter();
        _;
    }

    /**
     * @notice Initialize the contract
     * @param name Name of the Peons V2 Contract
     * @param symbol Symbol of the Peons V2 Contract
     * @param _lzEndpoint Address of LayerZero Endpoint
     * @param _royaltyReceiver Royalties receiver address
     * @param _collectionSize Collection size
     */
    constructor(
        string memory name,
        string memory symbol,
        address _lzEndpoint,
        address _royaltyReceiver,
        uint256 _collectionSize
    ) initializer {
        __OZNFTBase_init(name, symbol, _lzEndpoint, 500, address(bytes20("_joeFeeCollector")), _royaltyReceiver);
        collectionSize = _collectionSize;
    }

    //* External view functions */

    /**
     * @notice Returns tokenURI for a given tokenId from the Descriptor contract
     * @param tokenId The token id
     * @return tokenURI for a given tokenId
     */
    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        if (!_exists(tokenId)) revert PeonsV2__InvalidTokenId();
        return descriptor.tokenURI(tokenId);
    }

    /**
     * @notice Returns true if this contract implements the interface defined by
     * `interfaceId`. See the corresponding
     * https://eips.ethereum.org/EIPS/eip-165#how-interfaces-are-identified[EIP section]
     * to learn more about how these IDs are created.
     * This function call must use less than 30 000 gas.
     * @param interfaceId InterfaceId to consider. Comes from type(InterfaceContract).interfaceId
     * @return True if the considered interface is supported
     */
    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(IOZNFTBaseUpgradeable, OZNFTBaseUpgradeable)
        returns (bool)
    {
        return interfaceId == type(IPeonsV2).interfaceId || OZNFTBaseUpgradeable.supportsInterface(interfaceId);
    }

    //* External functions */

    /**
     * @notice Mints Peon V2
     * @dev Only callable by Peon Minter
     * @param user Address to mint Peon v2 to
     * @param tokenId Token ID to mint
     */
    function mint(address user, uint256 tokenId) external onlyPeonMinter {
        if (tokenId >= collectionSize) {
            revert PeonsV2__TokenIdOverCollectionSize();
        }
        _mint(user, tokenId);
    }

    //* External Owner functions */

    /**
     * @notice Set PeonMinter address
     * @dev Can only be called by the owner
     * @param _peonMinter address of PeonMinter to set
     */
    function setPeonMinter(address _peonMinter) external onlyOwner {
        if (_peonMinter == address(0)) revert PeonsV2__ZeroAddress();
        peonMinter = _peonMinter;
        emit PeonMinterSet(_peonMinter);
    }

    /**
     * @notice Set Descriptor address
     * @dev Can only be called by the owner
     * @param _descriptor address of Descriptor to set
     */
    function setDescriptor(address _descriptor) external onlyOwner {
        if (_descriptor == address(0)) revert PeonsV2__ZeroAddress();
        descriptor = IDescriptor(_descriptor);
        emit DescriptorSet(_descriptor);
    }
}

